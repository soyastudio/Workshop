					SET j = 1;
					service_contact : WHILE LASTMOVE(_service_contact) DO

							-- GetGroceryOrder/GroceryOrderData/GrocerySubOrder/CustomerService/PhoneFaxContact
							DECLARE PhoneFaxContact_1 REFERENCE TO CustomerService_.Item[j];
