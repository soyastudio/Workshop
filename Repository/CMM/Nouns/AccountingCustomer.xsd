	<xs:complexType name="CustomerGrpType">
		<xs:annotation>
			<xs:documentation>The AR Customer Group file contains customer group records. A customer group
defines the customer number/numbering conventions, and allows one or more
companies to use the same set of customers. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CompanyId"/>
			<xs:element ref="CustomerAccountNbr"/>
			<xs:element ref="AccountReceivableCd" minOccurs="0"/>
			<xs:element name="ARCustomerStatus" type="StatusType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the status of the customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrePaymentCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Prepayment code. This is the default code to be used for a check entered as a preayment. This will be the offset instead of RNA to cash.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NationalAccntInd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the customer is a national account customer. N = Parent and I = Invoiced</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ARCustomerBalances" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CurrentBalance" type="ARBalanceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is the current, non Bill of      Exchange balance for this customer,  cumulative for all companies utilizing this customer, in customer group currency.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DraftBalance" type="ARBalanceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is the current Bill of Exchange balance for this customer, cumulative                        for all companies utilizing this          customer, in customer group  currency.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ARCustomerLimits" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OrderLimit" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Order Limit in customer group currency for a given customer across all AR companies utilizing this customer.  </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CreditLimit" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Maximum amount of credit extended to a customer, in customer group currency, for all AR compaies utilizing this customer.        </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ARCustomerHoldCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer Hold Code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ARSalesRepresentativeCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The sales representative for this  invoice, debit memo or credit memo (note numeric)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AutoRemoveInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field determines whether payments and trransactions can be removed automatically (Y/N)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AutoCashInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the automatic cash application method for this customer. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ARCustomerStartDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date the customer first began doing business with the company.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineTax" type="TaxType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PaymentTerms" type="PaymentTermType" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation>Identifies the payment term for customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ARCustomerUser" minOccurs="0" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>Customer user-defined field</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerPartySitesType">
		<xs:annotation>
			<xs:documentation>Object used to identify various paty sites</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PartySiteId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Generic identifier field for party site. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PartySiteNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the Party Site.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="250"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MailStopCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A user-defined code to indicate a mail drop point within their organization.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PartySiteTypeCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Site use type, e.g. BILL_TO, SHIP_TO, SOLD_TO, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrimaryAddressInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicated if this is the identifying address for the party</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PrimaryInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that the record is marked as primary.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SiteLanguage" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Language of the translated text in Party Site.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Address" type="AddressType" minOccurs="0" maxOccurs="5"/>
			<xs:element name="Contact" type="AccountingCustomerContactType" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation>Contact person or party name</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerPartyType">
		<xs:annotation>
			<xs:documentation>Common object used to identify various business parties like, customer, shipper, vendor, bilto, ship to..etc</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PartyId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Generic identifier field for party. A party can have multiple types of identifiers that can be qualified by Type.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PartyNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of a party of the Organization party type.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="250"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PartyTypeCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The party type can only be Person, Organization, Group or Relationship. For Customer Import, only Person or Organization is applicable</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxTypeCd" default="" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DUNSNbr" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccoutingCustomerAddressType">
		<xs:annotation>
			<xs:documentation>Retail customer addresses and contact data.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0">
			<xs:element name="AddressId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique identifier of the address.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AddressNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the customer address.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AddressTypeDsc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Description of the customer address type.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Address" type="AddressType" minOccurs="0"/>
			<xs:element name="Notes" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Customer notes regarding the address.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="NoteTxt" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Text value of the note. Free format comments from customer.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="250"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
					<xs:attribute ref="typeCode"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="AddressPurposes" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Purpose of customer addresses.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="PurposeCd" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Code of the purpose.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="PurposeDsc" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Description of the address purpose.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
					<xs:attribute ref="typeCode"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="AddressStatuses" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="AddressStatusCd" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Address Status code.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AddressStatusDsc" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Description of the address status.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="200"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="AddressStatusVal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This field represents the address status value.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="SourcePlatformCd" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Code of the source platform.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="30"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="SourcePlatformDsc" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Name of the source platform.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="SourceStatusVal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This fields represents the status value for the source.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element ref="BannerCd" minOccurs="0"/>
						<xs:element name="StoreId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Store number.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:totalDigits value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="MessageTxt" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Address status message.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="200"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ReferenceId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Reference Id. Cross check with external systems.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="200"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute ref="typeCode"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SourceSystemType">
		<xs:annotation>
			<xs:documentation>Common type for specifying a specific individuals designator.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EntrySource" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This will be used to store the system ID (code) and system reference (desc) for each entity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BatchNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Batch identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute ref="typeCode">
			<xs:annotation>
				<xs:documentation>Attribute used to describe what is the type of the associated entity. This allows for extension of the model without the need for pre-defining a static ID element. For this BOD it can be used for the type of source.
Examples are: Party, Party Site, Account, Contact, Role, etc.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccountingCustomerBankType">
		<xs:annotation>
			<xs:documentation>Banktype will be used to store customer bank attributes such as bank codes, account numbers, bank routing numbers, bank adddress, region, branch information, account balance</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CompanyId" minOccurs="0"/>
			<xs:element name="BankCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation> A user-defined code representing a bank     account at a bank location. Also referred to as Cash Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAccntNbr" type="CodeWithDescription" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The bank-assigned account number identification to be used for the cash code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankId" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the ID number assigned to the bank by the Federal Reserve.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the bank name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankTransactionCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The bank transaction code. Represents a type of cash payment, bill of exchange    payment, or bank service. Also refered to as Bank-Inst code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransactionCdType" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the type of bank transaction Type . C = Cash Payment Transaction.  D = Bill of Exchange Payment Transaction. B = Bank Service Transaction.        </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransactionNbr" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique number used to identify bank transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAccntType" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the type of bank account used to deposit electronic payments for the vendor. Valid values are: C = Current/Checking Account.  S = Savings Account. Blank = Electronic payment type not  selected.          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransactionType" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the type of bank transaction. type Valid values are:  CR = Bank Credit.            DR= Bank Debit.    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransitNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A routing transit number is a nine-digit number used to identify a bank or financial institution when clearing funds for electronic transfers or processing checks in the United States.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankEFTNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Branch electronic fund transfer user number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankFormCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code used to drive the invoice number generation for transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankBranch" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank Branch information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankRegion" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank region information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAddress" type="AddressType" minOccurs="0"/>
			<xs:element name="AutomaticClearingHouseInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the value that indicates if the bank account allows ACH payments to   
be made. N = No ACH allowed  Y = ACH allowed</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankCurrencyCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The currency of the bank account.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankCurrencyExchangeRt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The exchange rate used to convert the transaction currency to the bank account currency.                   </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="18"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankEntity" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The bank location where the bank account is located</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendBankEntity" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the ID assigned by the Federal Reserve to the bank that processes the ACH tape.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendBankNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the name of the sending DFI bank.     </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankAccntStatus" type="StatusType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The status of the of the bank account. If the bank account is Inactive, new bank transactions cannot be entered for the bank account, but bank transactions already entered in the system can be processed and reconciled. Valid values
are: A = Active. I = Inactive.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankCodeInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the cash code has been used. Valid values are:  Y = Yes. N = No.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankClsCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank Class Code - The user class assigned to the cash code. Used to restrict access and use of the cash code to a specific group of
users. If left blank, no access
restrictions apply.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IBANId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>International bank account number.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EffetiveDateTimeType">
		<xs:annotation>
			<xs:documentation>Common type for Customer effective date and time.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0">
			<xs:element ref="FirstEffectiveDt" minOccurs="0"/>
			<xs:element ref="FirstEffectiveTm" minOccurs="0"/>
			<xs:element ref="LastEffectiveDt" minOccurs="0"/>
			<xs:element ref="LastEffectiveTm" minOccurs="0"/>
			<xs:element name="DurationNbr" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number that represents the duration.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DurationUnitDsc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Duration unit. D for Days, W for weeks.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="InclusiveInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether or not the dates and times are inclusive or not.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute ref="typeCode"/>
	</xs:complexType>
</xs:schema>
