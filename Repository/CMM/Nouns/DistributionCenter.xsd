<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSpy v2021 rel. 2 sp1 (x64) (http://www.altova.com) by Alex Nunes (Albertsons) --><xs:schema xmlns="http://collab.safeway.com/it/architecture/info/default.aspx" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:Abs="http://collab.safeway.com/it/architecture/info/default.aspx" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" targetNamespace="http://collab.safeway.com/it/architecture/info/default.aspx" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">	<xs:include schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>	<!--v1.0 - 7/7/2021 - Alex Nunes - Initial version.-->	<xs:complexType name="DistributionCenterType">		<xs:annotation>			<xs:documentation>Contains Data related to all distribution center which is an inventory storage location within a corporation. One or more logical warehouse facilities may exist           in a distribution center. Within a distribution center, any given item    may exist in only one logical warehouse facility.  </xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="CorporationId" nillable="false">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="3"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="DivisionId" nillable="false">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="3"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="PrimaryROGCd" minOccurs="0">				<xs:annotation>					<xs:documentation>Identifies the primary ROG for a particular Distribution Center</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="10"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="DistributionCenterId" nillable="true">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="10"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="DistributionCenterTypeCd">				<xs:annotation>					<xs:documentation>Type of DC. Values are: D – Direct delivery, W – Warehouse,S – Supply plant.</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="1"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="DistributionCenterNm" nillable="true" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element ref="Contact" minOccurs="0"/>			<xs:element name="OrgEntityType" type="OrgMasterNameType" minOccurs="0" maxOccurs="unbounded">				<xs:annotation>					<xs:documentation>Generic type to capture different org structures.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="EffectivePeriod" type="EffectiveDateTimeType" minOccurs="0"/>		</xs:sequence>	</xs:complexType></xs:schema>