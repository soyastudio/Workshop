<xs:schema xmlns:oa="http://www.openapplications.org/oagis/9" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://collab.safeway.com/it/architecture/info/default.aspx" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" targetNamespace="http://collab.safeway.com/it/architecture/info/default.aspx" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<!--V1.7 Alex Nunes 5/27/2021 - Removed ItemAvailabilityDtTm, Added  ProcessDt and OffsetDaysNbr at the header level   JIRA: EBC-14699 -->
	<!--V1.6 Alex Nunes 4/21/2021 - Added FacilityId in PurchaseOrder section at header level. JIRA: EBC-14204 -->
	<!--V1.5 Alex Nunes 3/19/2021 - Added DataBatchCd at detail level. JIRA: EBC-14130-->
	<!--V1.4 Alex Nunes 10/27/2020 - Added BatchExtract timestamp at header level JIRA: 12626.-->
	<!--V1.4 Rosabel Macario 11/05/2020 - Added tag to resolve dateTimeStamp issue-->
	<!--V1.3 Alex Nunes 9/2120 - Added Establishment Nbumber and Item Availability Date (Detail).-->
	<!--V1.2 Alex Nunes 8/18/20 - Added wholesale busines data for roll-up stores (Corp/Div/Fac).-->
	<!--V1.1 Patrick Oliver 7/22/20 - Updated VendorId & VendorSubAccountNbr to string(20)-->
	<!--04/07/20   Added DataBatchCd to OrderType-->
	<xs:include schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>
	<xs:element name="AdvanceShippingNotice" type="ASNType">
		<xs:annotation>
			<xs:documentation>root</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ASNType">
		<xs:annotation>
			<xs:documentation>Albertsons Advanced Shipping Notice busines object.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ASNHeader" type="ASNHeaderType">
				<xs:annotation>
					<xs:documentation>ASN Header data is basically at Voyage, BL, Container, PO level.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ASNDetail" type="ASNDetailType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>ASN detail data is at Pack/Item level within a BL, Container and PO.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Port">
		<xs:annotation>
			<xs:documentation>Defines a port, it is either water port or an inland location. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PortCd" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="ISOCd" use="optional">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="10"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PortNm" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CountryCd" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="ISOCd" use="optional">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="10"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CountryNm" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ASNHeaderType">
		<xs:annotation>
			<xs:documentation>Header information on Shipment</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Voyage" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies voyage details in ASN</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="VoyageNbr" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="VoyageNm" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="LoadPort" type="Port" minOccurs="0">
							<xs:annotation>
								<xs:documentation>location from where the goods are loaded into a container.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OriginPort" minOccurs="0">
							<xs:annotation>
								<xs:documentation>port of origin.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="Port"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="DischargePort" type="Port" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identiifes the discharge port where goods are discharged from a major vessel.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DestinationPort" type="Port" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identifies the final destination location.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VoyageModeCd" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identifies voyage transportation mode. 
ex by road, by sea, by air ..etc</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="RouteCd" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The route code is the order the stores are picked and is different by store.  </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="RouteStopId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Order of stop based on the route's store stops.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer"/>
							</xs:simpleType>
						</xs:element>
						<xs:element name="RouteNbr" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Unique number generated by transportaion system for the shipment delivery route.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="RouteDsc" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="250"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ETDDt" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Expected departure date and time.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DestETADt" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Expected arrival date and time at destination.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DischargeETADt" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Expected arrival date and time at discharge port.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Carrier" type="CarrierEquipment" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Idenitfies the party that serves as the carrier of the goods or services for the shipment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillOfLading" minOccurs="0">
				<xs:annotation>
					<xs:documentation>BL details in ASN.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BLNbr" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Bill of lading number only external customer delivery.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="30"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="BLDt" type="xs:date" minOccurs="0"/>
						<xs:element name="ShipmentNbr" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A unique control number assigned by the original shipper to identify a
specific shipment.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ShipmentDt" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Date of shipment. The actual date the goods are shipped.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ShipmentTm" type="xs:time" minOccurs="0"/>
						<xs:element name="ShipmentReferenceNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="SalesOrderNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="ShipFrom" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identifies the ShipFromParty for the document or the line. In a lot of cases, this is the same as the CustomerParty. In multi-organization purchasing, the CustomerParty may be different from the BuyerParty</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="CorporationId" minOccurs="0"/>
									<xs:element ref="DivisionId" minOccurs="0"/>
									<xs:element ref="DistributionCenterId" minOccurs="0"/>
									<xs:element ref="WarehouseId" minOccurs="0"/>
									<xs:element name="WarehouseAltId" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Alternate identifier for a warehouse like old warehouse id. or Id used in a legacy system.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="DistributionCenterAltId" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Alternate identifier for a DC like old DC id.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element ref="ROGCd" minOccurs="0"/>
									<xs:element name="ShipFromFacilityType" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates the type of the facility sending the ASN/shipment.
Ex: Abs Warehouse, Abs Manufacturing plant, vendor facility.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ShipFromFacilityId" type="xs:string" minOccurs="0"/>
									<xs:element name="ShipFromFacilityNm" type="xs:string" minOccurs="0"/>
									<xs:element name="ShipFromDUNSNbr" type="xs:string" minOccurs="0"/>
									<xs:element name="ShipFromContact" minOccurs="0" maxOccurs="2">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ContactNm" type="xs:string" minOccurs="0"/>
												<xs:element name="PhoneNbr" type="xs:string" minOccurs="0"/>
												<xs:element name="EmailAddresstxt" type="xs:string" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="ShipFromAddress" type="AddressType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ShipTo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identifies the ShipToParty for the document or the line. In a lot of cases, this is the same as the CustomerParty. In multi-organization purchasing, the CustomerParty may be different from the BuyerParty</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="CorporationId" minOccurs="0"/>
									<xs:element ref="DivisionId" minOccurs="0"/>
									<xs:element ref="DistributionCenterId" minOccurs="0"/>
									<xs:element ref="WarehouseId" minOccurs="0"/>
									<xs:element ref="ROGCd" minOccurs="0"/>
									<xs:element name="ShipToFacilityType" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates the type of the facility receiving the ASN/shipment.
Ex: Abs Warehouse, Abs Manufacturing plant, Abs Retail Store, External Customer.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ShipToFacilityId" type="xs:string" minOccurs="0"/>
									<xs:element name="ShipToFacilityNm" type="xs:string" minOccurs="0"/>
									<xs:element name="ShipToDUNSNbr" type="xs:string" minOccurs="0"/>
									<xs:element name="ShipToContact" minOccurs="0" maxOccurs="2">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ContactNm" type="xs:string" minOccurs="0"/>
												<xs:element name="PhoneNbr" type="xs:string" minOccurs="0"/>
												<xs:element name="EmailAddresstxt" type="xs:string" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="ShipToAddress" type="AddressType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Consignee" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Consignee is the person to whom the carrier (Ship) is supposed to deliver the goods as per contract of carriage.
In most cases the consignee is the Buyer of the goods but not always.
Consignee could be the agent nominated by the buyer or even a bank. 
</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ConsigneeCd" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="20"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="ConsigneeNm" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="ConsigneeDUNSNbr" type="xs:string" minOccurs="0"/>
									<xs:element name="ConsigneeContact" minOccurs="0" maxOccurs="2">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ContactNm" type="xs:string" minOccurs="0"/>
												<xs:element name="PhoneNbr" type="xs:string" minOccurs="0"/>
												<xs:element name="EmailAddresstxt" type="xs:string" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="ConsigneeAddress" type="AddressType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="NotifyParty" minOccurs="0" maxOccurs="3">
							<xs:annotation>
								<xs:documentation>Notify party is the party to whom the carrier is suppose to notify regarding the arrival ETA’s of the shipment.
 Notify party is then responsible for arranging the arrival formalities of the vessel.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="NotifyPartyCd" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="20"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="NotifyPartNm" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="NotifyPartyDUNSNbr" type="xs:string" minOccurs="0"/>
									<xs:element name="NotifyPartyAddress" type="AddressType" minOccurs="0"/>
									<xs:element name="NotifyPartyContact" minOccurs="0" maxOccurs="2">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ContactNm" type="xs:string" minOccurs="0"/>
												<xs:element name="PhoneNbr" type="xs:string" minOccurs="0"/>
												<xs:element name="EmailAddresstxt" type="xs:string" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="BLNotesTxt" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="BLCommentTxt" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Container" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipping conaitner for inter modal freight transport.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ContainerNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="ContainerTypeCd" type="xs:string" minOccurs="0"/>
						<xs:element name="ContainerTypeDsc" type="xs:string" minOccurs="0"/>
						<xs:element name="MaxVolume" minOccurs="0">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:decimal">
										<xs:attribute name="Unit" type="xs:string"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="MaxWeight" minOccurs="0">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:decimal">
										<xs:attribute name="Unit" type="xs:string"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="RefrigiratedInd" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PurchaseOrder" type="OrderType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about Purchase Order related to ASN.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestedDeliveryDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Requested date of delivery for the shipment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestedShipmentDtTm" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date and time of when the shipment is supposed to be ready for delivery.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PromisedDeliveryDtTm" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Promised date and time of when the shipment is delivered.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PromisedShipmentDtTm" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Promised date and time of when the shipment is shipped.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BombingProcessingDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the date that the order was put through the Bombing process. The date that the warehouse process the order.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProcessDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Process date of the store order.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OffsetDaysNbr" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number of days from the day items were picked up to the day items will be available on store shelf.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EffectiveBillingDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date that the order will arrive at the store. Stores are billed based on this date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Vendor" type="VendorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>It can have information about Vendor, Vendor Sub Account or Warehouse Vendor Sub account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Supplier" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SupplierId" type="xs:string" minOccurs="0"/>
						<xs:element name="SupplierDUNSNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="SupplierNm" type="xs:string" minOccurs="0"/>
						<xs:element name="SupplierContact" minOccurs="0" maxOccurs="2">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ContactNm" type="xs:string" minOccurs="0"/>
									<xs:element name="PhoneNbr" type="xs:string" minOccurs="0"/>
									<xs:element name="EmailAddresstxt" type="xs:string" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Address" type="AddressType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Customer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer Party that received goods and services from Albertson companies manufacturing plants.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CustomerId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Uniquely identifier for Customer. </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:maxInclusive value="99999999"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="CustomerAccountNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="SubCustomerId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Uniquely identifier for Sub-Customer. This could be a branch, subsidiary, etc... </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:maxInclusive value="99999999"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="CustomerTypeCd" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This code indicates the type of customer. Values: A - donations to outside agencies C - cafeteria for division office services. D - cafeteria for division office. E - donations to Safeway Employee Association. P - sales to the public. S - Safeway store. W - cafeteria for warehouse only.  </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="CustomerFacilityId" type="xs:string" minOccurs="0"/>
						<xs:element name="CustomerFacilityNm" type="xs:string" minOccurs="0"/>
						<xs:element name="CustomerDUNSNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="CustomerContact" minOccurs="0" maxOccurs="2">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ContactNm" type="xs:string" minOccurs="0"/>
									<xs:element name="PhoneNbr" type="xs:string" minOccurs="0"/>
									<xs:element name="EmailAddresstxt" type="xs:string" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="CustomerAddress" type="AddressType" minOccurs="0"/>
						<xs:element name="CustomerScheduleCd" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This code indicates the status of how the order was received at the Tandem. Values: E - Scheduled order; all expense items  F - Unscheduled order; all expense items. N - Unscheduled order. Y - Scheduled order </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BillTo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Party responsible for the payment as per PO or Invoice terms.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BillToFacilityId" type="xs:string" minOccurs="0"/>
						<xs:element name="BillToFacilityNm" type="xs:string" minOccurs="0"/>
						<xs:element name="BillToDUNSNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="BillToContact" minOccurs="0" maxOccurs="2">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ContactNm" type="xs:string" minOccurs="0"/>
									<xs:element name="PhoneNbr" type="xs:string" minOccurs="0"/>
									<xs:element name="EmailAddresstxt" type="xs:string" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="BillToAddress" type="AddressType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Buyer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Party responsible for ordering the Qty. It could be a Buyer, Forecaster or any other party.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BuyerId" type="xs:string" minOccurs="0"/>
						<xs:element name="BuyerNm" type="xs:string" minOccurs="0"/>
						<xs:element name="FacilityId" type="xs:string" minOccurs="0"/>
						<xs:element name="BuyerPhoneNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="BuyerEmailId" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ShipmentLadingQty" type="xs:decimal" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number of units (pieces) of the lading commodity. The bill of lading is a required document to move a freight shipment. The bill of lading works as a receipt of freight services, a contract between a freight company and shipper.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderLadingQty" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Order lading quantity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipmentGrossWgt" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipment gross weight.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightUOMCd" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="WeightQualifierCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code defining the type of weight</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipmentVol" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the total cube (number of feet measure cubed) in the order.  It is a total of what was in the original order, plus what was added or subtracted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VolumeUOMCd" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TotalNumberPallets" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the total number of pallets in the order.  It is a total of what was in the original order plus what was added or subtracted (in the ADDED_PALLETS field). </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="2"/>
						<xs:maxInclusive value="9999999"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PackagingCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code identifying the type of packaging; Part 1:
Packaging Form, Part 2: Packaging Material; if the
Data Element is used, then Part 1 is always
required
1P0A3C •K PAAGCINKGA GCIONDGE C •O 1D0E3
TRANSACTION SETS USED</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ManufacturerId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique identifier of the manufacturer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransportationTerms" type="TransportationTermType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Defines tranportation terms like inco terms details.
ex FOB, CIS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipmentInstructions" minOccurs="0" maxOccurs="30">
				<xs:annotation>
					<xs:documentation>General instructions on shipment.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Type" type="xs:string"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="MiscellaneousTxt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Generic description. Some systems might use this field.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="USServiceDiv" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the US servicing division that the Tandem uses.  It is the last two digits of the Tandems four digit number.       </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:maxInclusive value="99"/>
						<xs:totalDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PriceAreaCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This represents a group of stores who charge the same price for the item. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AssignmentId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Assignment id with in shipping location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExtractPrintCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates if the invoice had to be finalized automatically.  </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="Status" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the status of ASN as 
 sent,
 received,
 closed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WholesaleBusinessData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Wholesale business sales office.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="WholesaleCorporationId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Corporation identifier for the wholesale sales office.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="WholesaleDivisionId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Division identifier for the wholesale sales office.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="WholesaleROGCd" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ROG code for the wholesale sales office.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="WholesaleFacilityId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Facility ID for the wholesale sales office.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BatchExtractTs" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date and time of when retail inventory data batch was extracted. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrgMasterNameType" type="OrgMasterNameType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ASNDetailType">
		<xs:annotation>
