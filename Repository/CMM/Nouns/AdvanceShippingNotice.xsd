								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="ShippedDateTime" minOccurs="0">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:dateTime">
										<xs:attribute name="Qualifier" type="xs:string"/>
