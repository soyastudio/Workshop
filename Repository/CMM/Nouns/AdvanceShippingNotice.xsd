								<xs:documentation>Identifying number for Purchase Order assigned by the
                       orderer/purchaser</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PurchaseOrderDt" type="xs:date" minOccurs="0"/>
						<xs:element name="VendorOrderNbr" type="xs:string" minOccurs="0"/>
						<xs:element name="ReferenceID" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="Qualifier" type="xs:string"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<!--xs:element name="DeliveryRequestedDateTime" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:dateTime">
										<xs:attribute name="Qualifier" type="xs:string"/>
