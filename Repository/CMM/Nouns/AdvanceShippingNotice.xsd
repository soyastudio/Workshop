					<xs:documentation>Hierarchy of the shipment per an order, like tare, pallet, pack and item details.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<!--xs:element name="OrderLadingQty" type="xs:string"/>
						<xs:element name="PackingCd" type="xs:string" minOccurs="0"/>
						<xs:element name="LadingDescription" type="xs:string" minOccurs="0"/>
