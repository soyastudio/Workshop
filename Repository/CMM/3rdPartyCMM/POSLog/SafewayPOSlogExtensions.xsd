			<xs:element name="TaxPlanA" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan A applies to this item.Source field in Tlog : Item Indicat1 - Bit24</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPlanB" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan B applies to this item.Source field in Tlog : Item Indicat1 - Bit25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPlanC" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan C applies to this item.Source field in Tlog : Item Indicat1 - Bit26</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPlanD" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan D applies to this item.Source field in Tlog : Item Indicat1 - Bit27</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="FoodStampItem" minOccurs="0"/>
			<xs:element ref="NonDiscountable" minOccurs="0"/>
			<xs:element ref="CouponMultiplication" minOccurs="0"/>
			<xs:element ref="OverrideRequired" minOccurs="0"/>
			<xs:element ref="MultiPriced" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Multi Priced Item. Multipricing group number / Mix and match number (used when a
combination of items compose a deal; value 0-99, 0 = mix only with self) Source field in Tlog : Item Indicat2 - Bit4</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="WeightOrQuantity" minOccurs="0"/>
			<xs:element ref="StoreCouponCreated" minOccurs="0"/>
			<xs:element ref="TaxKey" minOccurs="0"/>
			<xs:element ref="FoodStampKey" minOccurs="0"/>
			<xs:element ref="CancelKey" minOccurs="0"/>
			<xs:element ref="RefundKey" minOccurs="0"/>
			<xs:element ref="StoreCouponKey" minOccurs="0"/>
			<xs:element name="TaxPlanE" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan E applies to this item.Source field in Tlog : Item Indicat? - Bit??</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPlanF" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan F applies to this item.Source field in Tlog : Item Indicat? - Bit??</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPlanG" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan G applies to this item.Source field in Tlog : Item Indicat? - Bit??</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPlanH" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that Tax Plan H applies to this item.Source field in Tlog : Item Indicat? - Bit??</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MultiPriceGroupNbr" type="xs:string" minOccurs="0"/>
			<xs:element name="DealQuantity" type="xs:integer" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number of items in the deal up to this point.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PriceMethod" type="xs:string" minOccurs="0"/>
			<xs:element name="SaleQuantity" type="xs:integer" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pricing quantity from the item record count or weight of items in the deal
(value 0-99, 0 is equivalent to 1).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SalePrice" type="xs:decimal" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pricing data from the item record Pricing method 0 or 1: format
00pppppppp where:
pppppppp = unit price if SALEQUAN = 0 or 1
= deal price if SALEQUAN &gt; 1
Pricing method 2: format pppppsssss where:
ppppp = package deal price
sssss = single unit price
Pricing method 3 or 4 format uuuuurrrrr where:
uuuuu = unit price
rrrrr = reduced price</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MeasuredQuantity" type="xs:decimal" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Entered item quantity or weight or volume.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="WeightKeyed" minOccurs="0"/>
			<xs:element ref="QuantityKeyed" minOccurs="0"/>
			<xs:element ref="TareKeyed" minOccurs="0"/>
			<xs:element ref="ScaleWeight" minOccurs="0"/>
			<xs:element ref="QuantityRequired" minOccurs="0"/>
			<xs:element ref="RepeatQuantity" minOccurs="0"/>
			<xs:element ref="CouponQuantity" minOccurs="0"/>
			<xs:element ref="QuantityEntryMethod" minOccurs="0"/>
			<xs:element name="PAGItemInfo" type="PAGExtensionsType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SwyTxnExtensionsType">
		<xs:annotation>
			<xs:documentation>           This is a container for all Safeway extensions to POSLog Transaction.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SwyTransactionID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Key to uniquely identify the POSLog transactions. It's a combination of BatchId + WorkstationId+SequenceNumber generated by the workstation for the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RingTime" type="xs:decimal" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total Ring time for the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TenderTime" type="xs:decimal" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total Tender time for the Transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LockTime" type="xs:decimal" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total Terminal Lock time in the Transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdleTime" type="xs:decimal" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total Idle time of the terminal within the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TOF" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates TOF - Terminal Offline, Recovery Transaction. 
Source field in Tlog : Hdr Indicat1 - Bit1</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreviousTLOGCloseString" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Transaction copied after a previous TLOG close indicator.
                      Source field in Tlog : Hdr Indicat1 - Bit2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalRecords" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates additional records for this transaction.
Source field in Tlog : Hdr Indicat1 - Bit12</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NotFirstRecord" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not first record for this transaction Indicator
                        Source Field in Tlog : Hdr Indicat1 - Bit13</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnTransfered" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that the Txn was transfered from a different workstation
                      Source Field in Tlog : Hdr Indicat1 - Bit18</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnMonitored" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Source Field in Tlog : Hdr Indicat1 - Bit19</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TerminalAcct" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the Terminal accountability of the cash register.
Source Field in Tlog : Hdr Indicat1 - Bit31</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TenderRejected" type="xs:boolean" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tender Rejected indicator. Source field inTLog : Hdr Indicat1 - Bit26</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransactionType" type="xs:string"/>
			<xs:element ref="EcommOrderNumber" minOccurs="0"/>
			<xs:element name="BusinessPartner" type="BusinessPartnerType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
