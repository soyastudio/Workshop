				</xs:annotation>
			</xs:element>
			<xs:element ref="FuelItem" minOccurs="0"/>
			<xs:element ref="LogAll" minOccurs="0"/>
			<xs:element ref="LogException" minOccurs="0"/>
			<xs:element ref="AliasItemFile" minOccurs="0"/>
			<xs:element ref="NoItemMovement" minOccurs="0"/>
