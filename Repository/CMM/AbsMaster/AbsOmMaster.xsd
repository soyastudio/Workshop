					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ReplacedCorporationId" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Captures the store id changes. Represents replaced Corp Id of the replaced store.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ReplacedDivisionId" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Captures the store id changes. Represents replaced Division Id of the replaced store.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ScanEquipmentVendorCd" nillable="true" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="18"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CompanyNbr" type="xs:string" minOccurs="0"/>
			<xs:element name="AcquiredFromNm" type="xs:string" minOccurs="0">
