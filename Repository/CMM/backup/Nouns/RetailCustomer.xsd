											<xs:restriction base="xs:string">
												<xs:maxLength value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
								<xs:attribute ref="typeCode"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="DeliverySubscription" minOccurs="0">
							<xs:complexType>
								<xs:sequence minOccurs="0">
									<xs:element name="ServiceFeeWaivedInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicator of whether or not service fee is waived.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="DeliveryFeeWaivedInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicator of whether or not delivery fee is waived.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="FuelSurchargeWaivedInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicator of whether or not a surcharge on fuel is waived.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="MinimumBasketSizeQty" type="xs:int" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Threshold order amount to be eligible for the subscription. e.g.: minimum order amount must be $30 to be eligible for the subscription.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="AutoRenewInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicator of whether or not delivery subscription is auto renewable.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="AutoEnrollInd" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicator of whether or not auto enrollment is on.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Fee" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Plan actual fee.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="CurrencyCd" type="xs:string"/>
											<xs:attribute name="FeeAmt" type="xs:decimal"/>
										</xs:complexType>
									</xs:element>
									<xs:element name="InitialOrderAmountOff" minOccurs="0">
										<xs:annotation>
											<xs:documentation>At times, first time customer orders might be provided with a $'x' amount discount.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="CurrencyCd" type="xs:string"/>
											<xs:attribute name="DiscountAmt" type="xs:decimal"/>
										</xs:complexType>
									</xs:element>
									<xs:element name="SignupFee" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Signup fee is the discounted plan fee chosen opted by the customer.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="CurrencyCd" type="xs:string"/>
											<xs:attribute name="FeeAmt" type="xs:decimal"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CustomerPayment" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer payment information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
