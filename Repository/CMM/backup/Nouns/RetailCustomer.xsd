														<xs:restriction base="xs:string">
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="KeyAliasTxt" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Encryption key alias.</xs:documentation>
