															<xs:maxLength value="4"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="SaltTxt" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Encryption key salt text.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="50"/>
														</xs:restriction>
