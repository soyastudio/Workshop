														<xs:documentation>Encryption token Id.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="36"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
