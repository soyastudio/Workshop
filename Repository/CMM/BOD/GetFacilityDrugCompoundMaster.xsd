	<xs:import namespace="http://collab.safeway.com/it/architecture/info/default.aspx" schemaLocation="../Nouns/PharmacyMaster.xsd"/>
	<xs:import namespace="http://collab.safeway.com/it/architecture/info/default.aspx" schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>
	<xs:element name="GetFacilityDrugCompoundMaster" type="GetFacilityDrugCompoundMasterType">
		<xs:annotation>
			<xs:documentation>Business Object to define the pharmacy compound drug master data maintained in a phrmacy facility like a pharmacy store or Rx fulfillment center. Pharmacies prepare own compounds using different drug ingredients for certain prescriptions with various strengths and dosages. These are called compounding drugs.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="GetFacilityDrugCompoundMasterType">
		<xs:annotation>
			<xs:documentation>Business Object to define the pharmacy compound drug master data maintained in a phrmacy facility like a pharmacy store or Rx fulfillment center. Pharmacies prepare own compounds using different drug ingredients for certain prescriptions with various strengths and dosages.</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="DocumentData">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Document" type="Abs:DocumentType"/>
						<xs:element name="DocumentAction" type="Abs:DocumentActionType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FacilityDrugCompoundData" type="Abs:CompoundDrugType"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
