	<xs:import namespace="http://collab.safeway.com/it/architecture/info/default.aspx" schemaLocation="../Nouns/MadeToOrderPrepListEvent.xsd"/>
	<xs:element name="GetMadeToOrderPrepListEvent" type="GetMadeToOrderPrepListEventType">
		<xs:annotation>
			<xs:documentation>root</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="GetMadeToOrderPrepListEventType">
		<xs:annotation>
			<xs:documentation>Business Object Document for Albertsons Made To Order Fulfillment activities. Made To Orders are received from online customers thru various third party vendors or channels. Albertsons will receive these orders for fulfillment and customer delivery through various delivery service types. Made To Order fulfillment can have multiple assignments and events involved in it. Order created, staging, consolidation and delivery are different types of assginments involved in the process.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DocumentData">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Document" type="Abs:DocumentType"/>
						<xs:element name="DocumentAction" type="Abs:DocumentActionType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="MadeToOrderPrepListEventData" type="Abs:MadeToOrderPrepListEventType"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>