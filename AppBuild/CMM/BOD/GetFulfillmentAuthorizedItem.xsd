	<xs:import namespace="http://collab.safeway.com/it/architecture/info/default.aspx" schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>
	<xs:element name="GetFulfillmentAuthorizedItem" type="GetFulfillmentAuthorizedItem">
		<xs:annotation>
			<xs:documentation>root</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="GetFulfillmentAuthorizedItem">
		<xs:annotation>
			<xs:documentation>This object will items that are authorized to a Micro Fulfillment Center (MFC). Authorization data determines exactly which items are authorized within the MFC which can be leveraged by forecasting and replenishment applications. Authorized item data will include store and MFC locations, authorized item types, authorized item dates, and user audit data. </xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="DocumentData">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Document" type="Abs:DocumentType"/>
						<xs:element name="DocumentAction" type="Abs:DocumentActionType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FulfillmentAuthorizedItemData" type="Abs:FulfillmentAuthorizedItemType"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>