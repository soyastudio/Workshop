	<xs:element name="GetMarketPlaceTransaction" type="GetMarketPlaceTransactionType">
		<xs:annotation>
			<xs:documentation>Defines the online customer Transaction in Market place to buy items listed by different market place vendors. Market Place is an online platfarm where vendors host their virtual shops and offer their products to sell.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="GetMarketPlaceTransactionType">
		<xs:annotation>
			<xs:documentation>Defines the online customer Transaction in Market place to buy items listed by different market place vendors. Market Place is an online platfarm where vendors host their virtual shops and offer their products to sell.</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="DocumentData">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Document" type="Abs:DocumentType"/>
						<xs:element name="DocumentAction" type="Abs:DocumentActionType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="MarketPlaceTransactionData" type="Abs:MarketPlaceTransactionType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
