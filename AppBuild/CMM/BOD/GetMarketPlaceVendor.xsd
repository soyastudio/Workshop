	<!--V2.0 02/23/22 Patrick Oliver: New source MarketPlacer for object. Refer to noun MarketPlace and complex type MarketPlaceVendor  -->
	<xs:import namespace="http://collab.safeway.com/it/architecture/info/default.aspx" schemaLocation="../Nouns/MarketPlace.xsd"/>
	<xs:import namespace="http://collab.safeway.com/it/architecture/info/default.aspx" schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>
	<xs:element name="GetMarketPlaceVendor" type="GetMarketPlaceVendorType">
		<xs:annotation>
			<xs:documentation>Defines vendors participating in market place and selling their products. Market Place is online platform where vendors host their virtual shops and list the products they offer to sell. Note that within Market Place application term 'Vendor' can be referred to as 'Seller'. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="GetMarketPlaceVendorType">
		<xs:annotation>
			<xs:documentation>Defines vendors participating in market place and selling their products. Market Place is online platform where vendors host their virtual shops and list the products they offer to sell. Note that within Market Place application term 'Vendor' can be referred to as 'Seller'. </xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="DocumentData">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Document" type="Abs:DocumentType"/>
						<xs:element name="DocumentAction" type="Abs:DocumentActionType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="MarketPlaceVendorData" type="Abs:MarketPlaceVendorType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>