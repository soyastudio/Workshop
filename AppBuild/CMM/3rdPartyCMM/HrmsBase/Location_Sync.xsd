											<xsd:documentation>This denotes the  Safeway Division</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="S_FAC" type="xsd:short" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This denotes the Safeway Facility</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="S_OP_AREA" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This denotes the Safeway  Operating Area </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="S_DIST" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This denotes the Safeway District</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="S_OM_MAINTAINED" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This denotes the Org Master Maintained</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
