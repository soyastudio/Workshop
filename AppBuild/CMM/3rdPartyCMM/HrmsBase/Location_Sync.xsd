										<xsd:annotation>
											<xsd:documentation>This denotes the Daily Location Cutoff Time </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="S_W_CUTOFF_TM_DEC" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This denotes the Weekly Location Cutoff Time </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
								<xsd:attribute name="class" type="xsd:string"/>
