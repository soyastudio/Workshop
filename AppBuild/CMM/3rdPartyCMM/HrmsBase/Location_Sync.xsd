								<xsd:documentation>This denotes the Northern Ireland report Location</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="GVT_GEOLOC_CD" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This denotes the Geog Location Code</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="GVT_DESIG_AGENT" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>GVT_DESIG_AGENT denotes the GVT Designated Agent Code which is a character of length 4</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MATRICULA_NBR" type="xsd:byte" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>MATRICULA_NBR denotes the Matricula Number which is a number of length 5 with a decimal position of 0</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LABEL_FORMAT_ID2" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>LABEL_FORMAT_ID2 denotes the Carton Label Format ID which is a character of length 30</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LABEL_FORMAT_ID3" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>LABEL_FORMAT_ID3 denotes the Ship Container Label Format ID  which is a character of length 30</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="USG_LBL_FORMAT_ID" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>USG_LBL_FORMAT_ID denotes the Item Usage Label Format which is a character of length 30</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="FON_ER_ID_MEX" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This denotes the  Fonacot ID Number</xsd:documentation>
