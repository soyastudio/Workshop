										<xsd:annotation>
											<xsd:documentation>This denotes the Safeway Employee Association Sector</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="S_FAC_TYPE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This denotes the Facility Type</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
