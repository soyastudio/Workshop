								<xsd:documentation>ESTABID denotes the Establishment ID which is a character of length 12.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MESSAGE_TEXT2" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This denotes the Message text number</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="COMMENTS_2000" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>COMMENTS_2000 denotes the Comment Text which is a character of length 32700</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TELE_LOCATION" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This denotes tele_location</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LASTUPDDTTM" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Last updated Timestamp of the record</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
