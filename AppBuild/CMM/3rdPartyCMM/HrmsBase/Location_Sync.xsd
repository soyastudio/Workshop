								<xsd:documentation>This denotes the Local Tax</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LOC_TAX_SPCL_MEX" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This denotes the  Special Local Tax Establishment ID</xsd:documentation>
