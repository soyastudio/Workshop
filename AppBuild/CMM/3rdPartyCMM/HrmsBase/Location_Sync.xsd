								<xsd:documentation>This denotes the Spokesman Committee ID</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TARIFF_AREA_GER" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This denotes the Tarif Area</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TARIFF_GER" type="xsd:string" minOccurs="0">
