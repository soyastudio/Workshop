<?xml version="1.0"?>
<!-- edited with XMLSpy v2018 sp1 (x64) (http://www.altova.com) by Ashutosh Rana (TCS) -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://collab.safeway.com/it/architecture/info/pbsync" targetNamespace="http://collab.safeway.com/it/architecture/info/pbsync" elementFormDefault="qualified">
	<xsd:element name="S_PERSON_BASIC_SYNC_RES" type="S_PERSON_BASIC_SYNC_REStype"/>
	<xsd:complexType name="S_PERSON_BASIC_SYNC_REStype">
		<xsd:sequence>
			<xsd:element name="PERSON" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="EMPLID" type="xsd:int" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This field denotes Employee ID</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BIRTHDATE" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This field denotes Date of Birth.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BIRTHPLACE" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This field denotes Place of Birth</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BIRTHCOUNTRY" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This field denotes Country of Birth</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BIRTHSTATE" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This field denotes State of Birth</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DT_OF_DEATH" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>This field denotes Date of Death.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LAST_CHILD_UPDDTM" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Used to indicate the last update to any data belonging to this logical object. For PER_ORG_ASGN - this means any update to its children in JOB_DATA that do not have their own date.  For PERSON - this means any update to its children in PERSONAL_DATA that do not have their own date.</xsd:documentation>
								<xsd:documentation>LAST_CHILD_UPDDTM is a datetime.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PERS_DATA_EFFDT" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="EMPLID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes EmployeeID</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EFFDT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes Effective Date</xsd:documentation>
											<xsd:documentation>EFFDT is a date.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="MAR_STATUS" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Denotes Maital status of emloyee and is a character of length 1</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="MAR_STATUS_DT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This is the date that the employee's Marital status is/was effective.
/* Marital Status date is stored redundantly on PERS_DATA_EFFDT for two reasons. 
   1. To allow the date to be entered prior to the first PERS_DATA_EFFDT so that the actual date for the marital status on the hire row can be entered. This is the only time that the field is enterable by the user.
   2. To allow for easier reporting on the data via SQR and Crystal. This is denormalizing the PERS_DATA_EFFDT record, but the number of requests for this redundancy coupled with the need for #1, has provided the justification for denormalizing this information and taking on the added maintenance. */</xsd:documentation>
											<xsd:documentation>MAR_STATUS_DT is a date.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="SEX" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Denotes gender of the employee. Character of length 1.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HIGHEST_EDUC_LVL" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Denotes the Highest level of education of employee. Character of length 2.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="FT_STUDENT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Flag denotes Full-time student</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LANG_CD" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Language code</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ALTER_EMPLID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Alternate EmployeeID. Character of length 11</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LASTUPDDTTM" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Specifies the date and time of the last update to an entry.  This field is maintained by PeopleSoft and is used in a variety of contexts.</xsd:documentation>
											<xsd:documentation>LASTUPDDTTM is a datetime.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LASTUPDOPRID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Specifies the User ID which made the last update to an entry.  This field is maintained by PeopleSoft and is used in a variety of contexts.</xsd:documentation>
											<xsd:documentation>LASTUPDOPRID is a character of length 30</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="PERS_DATA_USF" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="EMPLID" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>EmployeeID</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="EFFDT" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Effective Date</xsd:documentation>
														<xsd:documentation>EFFDT is a date.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_CRED_MIL_SVCE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_CRED_MIL_SVCE is a character of length 6. Allows Numbers only</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_MILITARY_COMP" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_MILITARY_COMP is a character value indicating the type of Uniformed service exercised by a person like Air Force, Army,Coast Guard ,Marines or Navy.Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_MIL_GRADE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_MIL_GRADE indicates the Military grade value of an person. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_MIL_RESRVE_CAT" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_MIL_RESRVE_CAT indicates the Reserve Category provided for a person like army, airforce, navy or coast guard. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_MIL_SEP_RET" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_MIL_SEP_RET is a character value which denotes Military Separation Status of a person. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_MIL_SVCE_END" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_MIL_SVCE_END is a date value which indicates Military service end date of a person</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_MIL_SVCE_START" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_MIL_SVCE_START is a date value which indicates Military service end date of a person</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_MIL_VERIFY" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_MIL_VERIFY  indicates the Military Service verification number and a character of length 1. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_PAR_NBR_LAST" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_PAR_NBR_LAST identifies the Last personnel action request number which is a number of length 6 with a decimal position of 0</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_UNIF_SVC_CTR" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_UNIF_SVC_CTR is used to Notify the Military Pay Center of a person. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_VET_PREF_APPT" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_VET_PREF_APPT indicates Veterans Preference which is a character of length 1. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_VET_PREF_RIF" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_VET_PREF_RIF indicates Veterans Preference incase of Reduction in Force which is a character of length 1.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_CHANGE_FLAG" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_CHANGE_FLAG is used to indicate if there is any change in flag value and its a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_DRAFT_STATUS" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_DRAFT_STATUS is the draft status value which denotes whether it is 'R' registered or 'NR' NotRegistered or 'X' Not applicalable</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_YR_ATTAINED" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_YR_ATTAINED is a date value indicating the attained year</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="DISABLED_VET" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>DISABLED_VET indicates the Disabled Veteran which is a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="DISABLED" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>A flag that indicates whether or not (Y or N) a dependent is a disabled.</xsd:documentation>
														<xsd:documentation>DISABLED is a character of length 1. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_DISABILITY_CD" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_DISABILITY_CD denotes the disability code value which is a character of length 2</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GRADE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GRADE indicates the salary grade value of an employee which is  a character of length 3</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="SAL_ADMIN_PLAN" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>SAL_ADMIN_PLAN indicates the Salary Administration plan of an employee which is a character of length 4</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_CURR_AGCY_EMPL" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_CURR_AGCY_EMPL indicates whether he is a Current Agency Employee or not(Y/N) and it is  a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_CURR_FED_EMPL" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_CURR_FED_EMPL indicates whether he is a Current Federal Employee or not(Y/N) and it is a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_HIGH_PAY_PLAN" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_HIGH_PAY_PLAN indicates the Highest Pay Plan of an employee and it is a character of length 2</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_HIGH_GRADE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_HIGH_GRADE  indicates the Highest Grade value of an employee and it is a character of length 3</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_PREV_AGCY_EMPL" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_PREV_AGCY_EMPL indicates whether he is a Previous Agency Employee or not(Y/N) and itis a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_PREV_FED_EMPL" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_PREV_FED_EMPL indicates whether he is a Previous Federal Employee or not(Y/N) and itis a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_SEP_INCENTIVE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_SEP_INCENTIVE denotes Separation Incentive value of an employee which can be a Buyout or Early Retirement or Other Incentive reasons  and it is a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_SEP_INCENT_DT" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_SEP_INCENT_DT is the Separation Incentive Date of an employee</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_TENURE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_TENURE  indicates the Highest Career Tenure Value of an employee wheather it is permanent or conditional or indefinite type   and it is  a character of length 1</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="GVT_PAY_PLAN" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>GVT_PAY_PLAN is used to indicate the Pay Plan of an employee a character of length 2</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="BARG_UNIT" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>BARG_UNIT is a value used to define the bargaining unit which is a character of length 4</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="PERS_NID" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="EMPLID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes EmployeeID</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="COUNTRY" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This Field denotes Country</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="NATIONAL_ID_TYPE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes type of national identifier</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="NATIONAL_ID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes the value of national identifier</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="SSN_KEY_FRA" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes the key to link SSN for global payroll</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="PRIMARY_NID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes the Primary National identifier</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="TAX_REF_ID_SGP" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes the tax refund ID</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LASTUPDDTTM" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes the Last updated time of the record</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LASTUPDOPRID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>This field denotes the OPRID of individual who last updated the record.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="NAME_TYPE_VW" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="EMPLID" type="xsd:string" minOccurs="0"/>
									<xsd:element name="ORDER_BY_SEQ" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Added in Mouse Over Label</xsd:documentation>
											<xsd:documentation>ORDER_BY_SEQ is a number of length 2 with a decimal position of 0</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="NAME_TYPE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>NAME_TYPE is a character of length 3. Allows Uppercase characters including numbers</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="NAMES" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="EMPLID" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the EmployeeID</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME_TYPE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the type of the name</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="EFFDT" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the effective date</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="EFF_STATUS" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes Effective date Status</xsd:documentation>
														<xsd:documentation>EFF_STATUS is a character of length 1. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="COUNTRY_NM_FORMAT" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Supported Name Format Types</xsd:documentation>
														<xsd:documentation>COUNTRY_NM_FORMAT is a character of length 3. Allows Uppercase characters including numbers</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the Name of employee</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME_INITIALS" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the initails of name.</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME_PREFIX" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes prefix to the name</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME_SUFFIX" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the suffix to the name.</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME_ROYAL_PREFIX" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the Royalprefix to the name if present</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME_ROYAL_SUFFIX" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the royal suffix to the name if present</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NAME_TITLE" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>This field denotes the name title</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LAST_NAME_SRCH" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Last name without special characters</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="FIRST_NAME_SRCH" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>First name without special characters</xsd:documentation>
														<xsd:documentation>NAME_INITIALS is a character of length 6</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
