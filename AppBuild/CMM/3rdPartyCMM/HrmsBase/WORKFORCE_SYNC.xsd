<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://collab.safeway.com/it/architecture/info/wfsync" targetNamespace="http://collab.safeway.com/it/architecture/info/wfsync" elementFormDefault="qualified">
	<xsd:element name="S_WORKFORCE_SYNC_RES" type="S_WORKFORCE_SYNC_REStype"/>
	<xsd:complexType name="S_WORKFORCE_SYNC_REStype">
		<xsd:sequence>
			<xsd:element name="PER_ORG_ASGN" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="EMPLID" type="xsd:int" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>EMPLID is a field to store Applicant/Employee ID which is a character of length 11</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="EMPL_RCD" type="xsd:byte" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>EMPL_RCD is a field to store Employee Record which is a number of length 3</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PER_ORG" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>PER_ORG is field to store Organizational Relationship which a character of length 3</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ORG_INSTANCE_ERN" type="xsd:byte" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>ORG_INSTANCE_ERN is a field to store Organizational Instance which is  number of length 3</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="POI_TYPE" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>POI_TYPE is a field to store Person of Interest Type which is a character of length 5</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BENEFIT_RCD_NBR" type="xsd:byte" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>BENEFIT_RCD_NBR denotes Benefit Record Number which is a number of length 3</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="HOME_HOST_CLASS" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>HOME_HOST_CLASS denotes Home/Host Classification which is a character of length 1</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CMPNY_DT_OVR" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>CMPNY_DT_OVR denotes Override Company Sen. Dt which is character of length 1</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CMPNY_SENIORITY_DT" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>CMPNY_SENIORITY_DT denotes Company Seniority Date which is a date variable.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SERVICE_DT_OVR" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>SERVICE_DT_OVR denotes Override Service Date which is a character of length 1.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SERVICE_DT" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>SERVICE_DT denotes Service data of an employee which is a data variable.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SEN_PAY_DT_OVR" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>SEN_PAY_DT_OVR is a date which denotes Override Seniority Pay Date of an employee.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SENIORITY_PAY_DT" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>SENIORITY_PAY_DT is a date which denotes Seniority Pay Calc Date of an employee.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PROF_EXPERIENCE_DT" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>PROF_EXPERIENCE_DT is a date which denotes Professional Experience Date of an employee.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LAST_VERIFICATN_DT" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>LAST_VERIFICATN_DT is a date which describes Last Verification Date of an employee.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PROBATION_DT" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>PROBATION_DT is date which denotes Probation Date of an employee.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LAST_INCREASE_DT" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>LAST_INCREASE_DT is a date which denotes Date Last Increase of an employee.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="OWN_5PERCENT_CO" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>OWN_5PERCENT_CO is field which denotes whether the emplyoee Owns 5% (or More) of Company.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BUSINESS_TITLE" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>BUSINESS_TITLE is a field which stores the employee's Business Title.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="POSITION_PHONE" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>POSITION_PHONE is a field which stores the specific phone number of an employee.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LAST_CHILD_UPDDTM" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>LAST_CHILD_UPDDTM is a field which stores Last Update to Assignment Data.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PER_ORG_INST" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="EMPLID" type="xsd:int" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EMPLID indicates Employee Number.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ORG_INSTANCE_ERN" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ORG_INSTANCE_ERN is the field used to denote Organizational Instance (also referred to as an Employment Instance, a contingent worker instance, or a POI instance) it is number field oflength 3.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="PER_ORG" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>PER_ORG Defines the Organizational Relationship(s) that a Person has to the Organization.  These are Employee, Contingent Worker, and Persons of Interest.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="POI_TYPE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>POI_TYPE is the Person of Interest Type which is a categorization of the Person of Interest relationships, a person can have multiple POI_TYPES and some POI_TYPES require JOB information.A 5 length character.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ORIG_HIRE_OVR" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Indicates if Original hire date was overridden.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ORIG_HIRE_DT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ORIG_HIRE_DT denotes original hire date of the employee</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ORIG_INST_SRV_OVR" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ORIG_INST_SRV_OVR is used to indicate whether the user has overridden the Organizational Instance Service Date. If they have, then the field will not be set by the JOB peoplecode.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ORIG_INST_SRV_DT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ORIG_INST_SRV_DT indicates the Org Instance Service Date.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="NEE_PROVIDER_ID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>NEE_PROVIDER_ID indicates the ID for the Non Employee Provider, Agency, or Employer.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LASTUPDDTTM" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>LASTUPDDTTM is the Last updated timestamp.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LASTUPDOPRID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>LASTUPDOPRID indicates the Last updated by user information.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="JOB" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="EMPLID" type="xsd:int" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EMPLID is a field to store Applicant/Employee ID which is a character of length 11</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EMPL_RCD" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EMPL_RCD is a field to store Employee Record which is a number of length 3</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EFFDT" type="xsd:date" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EFFDT is a field to store effective date which denotes a date fron when the employee is active.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EFFSEQ" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EFFSEQ is a field to store effective sequence of effective dated employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="PER_ORG" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>PER_ORG is field to store person organisation.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="DEPTID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>DEPTID is field to store department code used in Albertsons.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="JOBCODE" type="xsd:short" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>JOBCODE is field to store job code used in Albertsons which describes various jobs.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="POSITION_NBR" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>POSITION_NBR is a field to store Position number of the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="SUPERVISOR_ID" type="xsd:int" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>SUPERVISOR_ID is a field to store the empl id of the supervisor for particular employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HR_STATUS" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>HR_STATUS is a field to store status of employee either active ot Inactive.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="APPT_TYPE" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>APPT_TYPE is a field to store the Appointment Type of the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="MAIN_APPT_NUM_JPN" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>MAIN_APPT_NUM_JPN  is a field to store Main Appointment Number for that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="POSITION_OVERRIDE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>POSITION_OVERRIDE  is a field to store Override Position Data of an employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="POSN_CHANGE_RECORD" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>POSN_CHANGE_RECORD is a field to store Position Management Record of an employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EMPL_STATUS" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EMPL_STATUS is a field to store status of an employee whether he is active/leave/retire.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ACTION" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ACTION is a field to store the recent action performed on the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ACTION_DT" type="xsd:date" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ACTION_DT is a field to store the date when the action is performed on the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ACTION_REASON" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ACTION_REASON is a field to store the action description performed on the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LOCATION" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>LOCATION is a field to the location code of that particular employee</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="TAX_LOCATION_CD" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>TAX_LOCATION_CD is the tax location of the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="JOB_ENTRY_DT" type="xsd:date" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>JOB_ENTRY_DT is a field to store the job entry date of an employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="DEPT_ENTRY_DT" type="xsd:date" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>DEPT_ENTRY_DT is a field to store when the employee enroled in that department.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="POSITION_ENTRY_DT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>POSITION_ENTRY_DT is a field to store the position entry date of an employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="SHIFT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>SHIFT is a field which denotes the shift of the employee either normal/night.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="REG_TEMP" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>REG_TEMP is a field which denotes that the employee is Regular/Temporary Position.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="FULL_PART_TIME" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>FULL_PART_TIME is a field which denotes the employee is a full time or part time employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="COMPANY" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>COMPANY denotes the company of that employee working.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="PAYGROUP" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>PAYGROUP denotes the paygroup of the company that employee belongs to.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="BAS_GROUP_ID" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>BAS_GROUP_ID is a field to store BAS Group ID of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG1" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG1 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG2" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG2 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG3" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG3 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG4" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG4 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG5" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG5 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG6" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG6 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG7" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG7 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG8" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG8 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ELIG_CONFIG9" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ELIG_CONFIG9 is a field to Eligibility Config Field of that employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="BEN_STATUS" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>BEN_STATUS is a field to store Benefits Employee  of the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="BAS_ACTION" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>BAS_ACTION is a field to store Benefits Administration Action of the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="COBRA_ACTION" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>COBRA_ACTION is a field to store COBRA_ACTION of the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EMPL_TYPE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EMPL_TYPE is a field to store Employee Type Default.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HOLIDAY_SCHEDULE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>HOLIDAY_SCHEDULE	denotes the Holiday Schedule ID of the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="STD_HOURS" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>STD_HOURS denotes the standard hours assigned to the employee.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="STD_HRS_FREQUENCY" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>STD_HRS_FREQUENCY indicates the Standard Work Period. It is 5 length Character.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="OFFICER_CD" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>OFFICER_CD describes the officer code. It is one length Character.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EMPL_CLASS" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EMPL_CLASS indicates Employee Classification whether Employee or contigent worker or POI.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="SAL_ADMIN_PLAN" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>SAL_ADMIN_PLAN is the Salary Administration Plan.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="GRADE" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>GRADE indicates the salary Grade.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="GRADE_ENTRY_DT" type="xsd:date" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>GRADE_ENTRY_DT gives the Salary GradeENtry Date.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="STEP" type="xsd:byte" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>STEP is a field created when save a job code is saved .The system uses rate code defaulting rules (instead of the Job Code and Salary Step setup tables) for the compensation defaulting.One length integer with 2 decimal places.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="STEP_ENTRY_DT" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>STEP_ENTRY_DT is the entry date of the step.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="GL_PAY_TYPE" type="xsd:short" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>GL_PAY_TYPE describes General Ledger Pay Type.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ACCT_CD" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>ACCT_CD indicates the Account or Combination Code.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="EARNS_DIST_TYPE" type="xsd:string" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>EARNS_DIST_TYPE indicates the Earnings Distribution Type.</xsd:documentation>
