            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="AccessLevelCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>A code that indicates what access privileges are granted to a
                        position.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="JobCode"
                type="xs:string">
                <xs:annotation>
                    <xs:documentation>The jobs which are allowed to work in this position, I.e.
                        operator, manager can both work the same position</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute default="false" form="unqualified" name="ShareFlag" type="xs:boolean"
            use="optional">
            <xs:annotation>
                <xs:documentation>An indicator for whether or not this position is eligible for job
                    sharing</xs:documentation>
            </xs:annotation>
        </xs:attribute>
