<?xml version="1.0" encoding="utf-8"?>

<xsd:schema
  version="1.0.8"
  attributeFormDefault="unqualified"
  elementFormDefault="qualified"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:mstns="http://www.safeway.com/Copient/Offer/Active/Extract/OfferExports.xsd">
  <xsd:element name="OfferExport">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="PublishingTopic" type="xsd:string" />
        <xsd:element name="ExportedAt" type="xsd:dateTime" />
        <xsd:element name="ID" type="xsd:string" />
        <xsd:element name="Note" type="xsd:string" minOccurs="0" />
        <xsd:element name="Version" type="xsd:string" fixed="1.0.8" />
        <xsd:element name="General">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Identification">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="OfferID" type="xsd:long" id="CPE_ST_Incentives.IncentiveID.2">
                      <!-- Description: Primary key for offer -->
                    </xsd:element>
                    <xsd:element name="ExternalID" id="CPE_ST_Incentives.ClientOfferID.3" minOccurs="0">
                      <!-- Description: PKID for offer in external system (eg OMS) -->
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:maxLength value="20" />
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="RewardOptionID" type="xsd:long" id="CPE_ST_RewardOptions.RewardOptionID.4"
                                 minOccurs="0">
                      <!-- Description: PKID for offer used in CPE -->
                    </xsd:element>
                    <xsd:element name="EngineID" type="xsd:integer" id="CPE_ST_Incentives.EnginePhraseID.5">
                      <!-- Description: 'Not really useful in Safeway, just NCR way of telling what kind of offer this is for, they have different engine in NCR, but Safeway only use CPE'
Linked to: PromoEngines -->
                    </xsd:element>
                    <xsd:element name="EngineName" type="xsd:string"
                                 id="CPE_ST_Incentives.Phrase.EnginePhraseID.6">
                      <!-- Field link: CPE_ST_Incentives.EnginePhraseID=PhraseText.PhraseID -->
                    </xsd:element>
                    <xsd:element name="Status">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.StatusFlag.7" />
                          <xsd:element name="Name" id="ProductTypes.Name.7">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="255" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Name" id="CPE_ST_Incentives.IncentiveName.8">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:maxLength value="100" />
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="Description" id="CPE_ST_Incentives.Description.9">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:maxLength value="1000" />
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="Category">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.PromoClassID.10" />
                          <xsd:element name="Name" id="OfferCategories.Description.11">
                            <!-- Field link: CPE_ST_Incentives.PromoClassID=OfferCategories.OfferCategoryID -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="255" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="VendorCouponCode" id="CPE_ST_Incentives.VendorCouponCode.12">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:maxLength value="20" />
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Priority">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SelectOfferPriority">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.PriorityID.13" />
                          <xsd:element name="Name" id="CPE_IncentivePriorities.Description.14">
                            <!-- Description: 'High, Medium, Low'
Linked to: CPE_IncentivePriorities
Field link: CPE_ST_Incentives.PriorityID -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="50" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Dates">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ProductionStart" type="xsd:date" id="CPE_ST_Incentives.StartDate.15" />
                    <xsd:element name="ProductionEnd" type="xsd:date" id="CPE_ST_Incentives.EndDate.16" />
                    <xsd:element name="EligibilityStart" type="xsd:date"
                                 id="CPE_ST_Incentives.EligibilityStartDate.17" />
                    <xsd:element name="EligibilityEnd" type="xsd:date"
                                 id="CPE_ST_Incentives.EligibilityEndDate.18" />
                    <xsd:element name="TestingStart" type="xsd:date" id="CPE_ST_Incentives.TestingStartDate.19" />
                    <xsd:element name="TestingEnd" type="xsd:date" id="CPE_ST_Incentives.TestingEndDate.20" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="EligibilityFrequency">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Name" type="xsd:string"
                                 id="CPE_ST_Incentives.Phrase.P1DistQtyLimitP1DistPeriodP1DistTimeType.21">
                      <!-- Description: 'term.nolimit, onceperday, etc.: 1-5: no limit, once per transaction, once per day, once per week, once per offer'
Field link: CPE_DistributionTimeTypes.TimeTypeID=CPE_ST_Incentives.P1DistTimeType and PhraseText.PhraseID=CPE_DistributionTimeTypes.PhraseID -->
                    </xsd:element>
                    <xsd:element name="Limit" type="xsd:integer" id="CPE_ST_Incentives.P1DistQtyLimit.22">
                      <!-- Description: limit 1 fields -->
                    </xsd:element>
                    <xsd:element name="Period" type="xsd:integer" id="CPE_ST_Incentives.P1DistPeriod.23" />
                    <xsd:element name="Type">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.P1DistTimeType.24" />
                          <xsd:element name="Name" type="xsd:string"
                                       id="CPE_DistributionTimeTypes.Phrase.TimeTypeID.25" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="RewardFrequency">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Name" type="xsd:string"
                                 id="CPE_ST_Incentives.Phrase.P3DistQtyLimitP3DistPeriodP3DistTimeType.26">
                      <!-- Description: limit 3 fields
Field link: CPE_DistributionTimeTypes.TimeTypeID=CPE_ST_Incentives.P3DistTimeType and PhraseText.PhraseID=CPE_DistributionTimeTypes.PhraseID -->
                    </xsd:element>
                    <xsd:element name="Limit" type="xsd:integer" id="CPE_ST_Incentives.P3DistQtyLimit.27" />
                    <xsd:element name="Period" type="xsd:integer" id="CPE_ST_Incentives.P3DistPeriod.28" />
                    <xsd:element name="Type">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.P3DistTimeType.29" />
                          <xsd:element name="Name" type="xsd:string"
                                       id="CPE_DistributionTimeTypes.Phrase.TimeTypeID.30">
                            <!-- Field link: CPE_DistributionTimeTypes.TimeTypeID=CPE_ST_Incentives.TimeTypeID and PhraseText.PhraseID=CPE_DistributionTimeTypes.PhraseID -->
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="InboundOutbound">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="CreationSource">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.InboundCRMEngineID.31" />
                          <xsd:element name="Name" type="xsd:string"
                                       id="ExtCRMInterfaces.Phrase.InboundCRMEngineID.32">
                            <!-- Field link: ExtCRMInterfaces.ExtInterfaceId=CPE_ST_Incentives.InboundCRMEngineID -->
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="OutboundData">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.CRMEngineID.33" />
                          <xsd:element name="Name" type="xsd:string"
                                       id="ExtCRMInterfaces.Phrase.ExtInterfaceId.34">
                            <!-- Field link: Phrase for ExtInterfaceId=CPE_ST_Incentives.CRMEngineID -->
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="ChargebackVendor">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.ChargebackVendorID.35" />
                          <xsd:element name="Name" id="Vendors.Name.36">
                            <!-- Field link: CPE_ST_Incentives.ChargebackVendorID=Vendors.ExtVendorID -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="100" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Employees">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="AvailableToEmployeesOnly" type="xsd:boolean"
                                 id="CPE_ST_Incentives.EmployeesOnly.37">
                      <!-- Description: Employee enabled when EmployeesOnly Or EmployeesExcluded -->
                    </xsd:element>
                    <xsd:element name="NotAvailableToEmployees" type="xsd:boolean"
                                 id="CPE_ST_Incentives.EmployeesExcluded.38" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="AdvancedOptions">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="AutoTransferable" type="xsd:boolean"
                                 id="CPE_ST_Incentives.AutoTransferable.39" />
                    <xsd:element name="EnableIssuance" type="xsd:integer" id="CPE_ST_Incentives.SendIssuance.40" />
                    <xsd:element name="EnableImpressionReporting" type="xsd:boolean"
                                 id="CPE_ST_Incentives.EnableImpressRpt.41" />
                    <xsd:element name="EnableRedemptionReporting" type="xsd:boolean"
                                 id="CPE_ST_Incentives.EnableRedeemRpt.42" />
                    <xsd:element name="TreatAsManufacturerCoupon" type="xsd:integer"
                                 id="CPE_ST_Incentives.ManufacturerCoupon.43" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Summary">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Folders" id="Folders.FolderName.44" minOccurs="0">
                      <!-- Linked to: FolderItems
Field link: FoldereItems.LinkID=CPE_ST_Incentives.OfferID and FoldereItems.LinkTypeID=1 -->
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:maxLength value="50" />
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="Tiers" type="xsd:integer" id="CPE_ST_RewardOptions.TierLevels.45">
                      <!-- Field link: TouchResponse=0 and IncentiveID=OfferID -->
                    </xsd:element>
                    <xsd:element name="CreatedBy">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.CreatedByAdminID.46">
                            <!-- Linked to: AdminUsers
Field link: AdminUsers.AdminUserID=CPE_ST_Incentives.CreatedByAdminID -->
                          </xsd:element>
                          <xsd:element name="Name" id="AdminUsers.FirstNameLastName.47">
                            <!-- Linked to: AdminUsers
Field link: AdminUsers.AdminUserID=CPE_ST_Incentives.CreatedByAdminID -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="100" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="LastUpdatedBy">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer"
                                       id="CPE_ST_Incentives.LastUpdatedByAdminID.48">
                            <!-- Linked to: AdminUsers
Field link: AdminUsers.AdminUserID=CPE_ST_Incentives.LastUpdatedByAdminID -->
                          </xsd:element>
                          <xsd:element name="Name" id="AdminUsers.FirstNameLastName.49">
                            <!-- Linked to: AdminUsers
Field link: AdminUsers.AdminUserID=CPE_ST_Incentives.LastUpdatedByAdminID -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="100" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Deleted" type="xsd:integer" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Conditions">
          <xsd:complexType>
            <xsd:all>
              <xsd:element name="CustomerConditions" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SelectedCustomerGroup" minOccurs="1" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.RewardOptionID.50">
                            <!-- Description: ExcludedUsers=0 -->
                          </xsd:element>
                          <xsd:element name="Name" id="CustomerGroup.Name.51">
                            <!-- Description: ExcludedUsers=0
Field link: 'select CG.CustomerGroupID, Name from CPE_ST_IncentiveCustomerGroups as ICG with (NoLock) left join CustomerGroups as CG with (NoLock) on CG.CustomerGroupID=ICG.CustomerGroupID where RewardOptionID=@RewardOptionID and ICG.deleted=0 and ExcludedUsers=0 and ICG.CustomerGroupID is not null' -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="255" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="ExcludedCustomerGroup" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.RewardOptionID.52">
                            <!-- Description: ExcludedUsers=1 -->
                          </xsd:element>
                          <xsd:element name="Name" id="CustomerGroup.ExcludedName.53">
                            <!-- Description: ExcludedUsers=1
Field link: 'select CG.CustomerGroupID, Name from CPE_ST_IncentiveCustomerGroups as ICG with (NoLock) left join CustomerGroups as CG with (NoLock) on CG.CustomerGroupID=ICG.CustomerGroupID where RewardOptionID=@RewardOptionID and ICG.deleted=0 and ExcludedUsers=1 and ICG.CustomerGroupID is not null' -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="255" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="EnableHouseholding" type="xsd:boolean"
                                 id="CPE_ST_RewardOptions.HHEnable.54" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="ProductConditions" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ProductCondition" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="SelectedProductGroup" minOccurs="0">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ID" type="xsd:integer"
                                             id="CPE_ST_IncentiveProductGroups.ProductGroupID.55">
                                  <!-- Description: ExcludedProducts=0 -->
                                </xsd:element>
                                <xsd:element name="Name" id="ProductGroups.Name.56">
                                  <!-- Description: ExcludedProducts=0
Linked to: CPE_ST_IncentiveProductGroups
Field link: 'select PG.ProductGroupID,Name from CPE_ST_IncentiveProductGroups as IPG left join ProductGroups as PG on PG.ProductGroupID=IPG.ProductGroupID where RewardOptionID=@RewardOptionID and IPG.deleted=0 and ExcludedProducts=0 and IPG.ProductGroupID is not null' -->
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                      <xsd:maxLength value="200" />
                                    </xsd:restriction>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="Items" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                          <xsd:attribute name="Type" type="xsd:integer" />
                                          <xsd:attribute name="ID" type="xsd:string" />
                                          <xsd:attribute name="Description" type="xsd:string" />
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="ExcludedProductGroup" minOccurs="0">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ID" type="xsd:integer" id="CPE_ST_Incentives.ProductGroupID.57">
                                  <!-- Description: ExcludedProducts=1 -->
                                </xsd:element>
                                <xsd:element name="Name" id="ProductGroups.Name.58">
                                  <!-- Description: ExcludedProducts=1
Field link: 'select PG.ProductGroupID,Name from CPE_ST_IncentiveProductGroups as IPG left join ProductGroups as PG on PG.ProductGroupID=IPG.ProductGroupID where RewardOptionID=@RewardOptionID and IPG.deleted=0 and ExcludedProducts=1 and IPG.ProductGroupID is not null' -->
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                      <xsd:maxLength value="200" />
                                    </xsd:restriction>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="Items" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                          <xsd:attribute name="Type" type="xsd:integer" />
                                          <xsd:attribute name="ID" type="xsd:string" />
                                          <xsd:attribute name="Description" type="xsd:string" />
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Quantity" type="xsd:decimal"
                                       id="CPE_ST_IncentiveProductGroups.QtyForIncentive.59" />
                          <xsd:element name="Unit">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ID" type="xsd:integer"
                                             id="CPE_ST_IncentiveProductGroups.QtyUnitType.60" />
                                <xsd:element name="Name" id="CPE_UnitTypes.Description.61">
                                  <!-- Field link: 'select UnitTypeID, PhraseID, Description from CPE_UnitTypes UT with (NoLock) where MultiUOMState in (-1,0);' -->
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                      <xsd:maxLength value="50" />
                                    </xsd:restriction>
                                  </xsd:simpleType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="UniqueProduct" type="xsd:boolean"
                                       id="CPE_ST_IncentiveProductGroups.UniqueProduct.62" />
                          <xsd:element name="Minimum" type="xsd:decimal"
                                       id="CPE_ST_IncentiveProductGroups.MinPurchAmt.63" />
                          <xsd:element name="Limit" type="xsd:decimal" id="CPE_ST_IncentiveProductGroups.AccumLimit.64">
                            <!-- Description: per -->
                          </xsd:element>
                          <xsd:element name="Accumulation">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="EnableAccumulation" type="xsd:boolean"
                                             id="CPE_ST_IncentiveProductGroups.AccumMin.65">
                                  <!-- Description: AccumMin > 0 -->
                                </xsd:element>
                                <xsd:element name="Period" type="xsd:integer"
                                             id="CPE_ST_IncentiveProductGroups.AccumPeriod.66">
                                  <!-- Description: days -->
                                </xsd:element>
                                <xsd:element name="Disqualifier" type="xsd:string"
                                             id="CPE_ST_IncentiveProductGroups.Disqualifier.66">
                                  <!-- Description: Disqualifier -->
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="ProductConditionsJoinType" type="xsd:string"
                           id="CPE_ST_RewardOptions.ProductComboID.66.5" minOccurs="0" />
              <xsd:element name="PointConditions" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SelectedPointProgram" minOccurs="0">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer"
                                       id="CPE_ST_IncentivePointsGroups.ProgramID.67">
                            <!-- Description: Doesn't actually have a label -->
                          </xsd:element>
                          <xsd:element name="Name" id="PointPrograms.ProgramName.68">
                            <!-- Description: Doesn't actually have a label
Field link: 'Select IPG.ProgramID,ProgramName from CPE_ST_IncentivePointsGroups as IPG with (NoLock) left join PointsPrograms as PP with (NoLock) on PP.ProgramID=IPG.ProgramID where IPG.deleted=0 and IPG.ProgramID is not null and RewardOptionID=@RewardOptionID' -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="255" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="ValueNeededToSatisfyCondition" type="xsd:integer"
                                 id="CPE_ST_IncentivePointsGroups.QtyForIncentive.69" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="DayConditions" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SelectedDay" minOccurs="0" maxOccurs="7">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ID" type="xsd:integer" id="CPE_DaysOfWeek.DOWID.70">
                            <!-- Description: '1=Sunday, 2=Monday, etc' e-->
                          </xsd:element>
                          <xsd:element name="Name" id="CPE_DaysOfWeek.DayName.71">
                            <!-- Field link: 'select DOWID, DayName, PhraseID from CPE_DaysOfWeek DW with (NoLock)' -->
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="50" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="TimeConditions" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
