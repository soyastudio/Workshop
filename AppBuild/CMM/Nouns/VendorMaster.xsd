	<xs:element name="VendorMaster" type="VendorCorporateType">
		<xs:annotation>
			<xs:documentation>Root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="VendorCorporateType">
		<xs:annotation>
			<xs:documentation>Albertson's Business Object for Corporate Level Vendor. This is the top level of company's Vendor structure. In SSIMS this type of data is being stored in ARPMAS table.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VendorId" nillable="false">
				<xs:annotation>
					<xs:documentation>Vendor Identifier. It&apos;s a six character field with zeros (left) padded.&#10;</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorNm" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the Vendor</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorSearchNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The search name for the vendor.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorLegalNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The legal name for the vendor. Used for reporting reportable vendor income. If left blank, the Vendor Name defaults.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorMainPhoneNbr" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Main Vendor phone number.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorStatus" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor Status</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorStatusDsc" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorTypeCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor type. Sample value 'Supplier'</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorOutlet" type="VendorOutletType" minOccurs="0"/>
			<xs:element name="VendorContact" type="VendorContactType" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation>Vendor Contact.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorEffectiveDt" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Effective date of the Vendor </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorSetupDt" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="LastFileMaintenanceDt" type="xs:dateTime" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>When was the last file Maintenance Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CreditBureauId" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifer of the Credit Bureau</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankNbr" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank Number</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankBranchNbr" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Branch Number</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankAccnt" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank Account</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankAccntType" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank Account Type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankDataPhoneNbr" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Phone Number (Data) of the Bank.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ElectronicDataCd" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Electronic Data Code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AccountReceivableClassifCd" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Account Receivable Classification Code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AccountPayableClassifCd" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Account Payable Classification Code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VolumeRebateInd" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Volume Rebate Indicator</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorYearEndDt" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor Year End Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastYearsVolumeRebatePct" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Last Years Volume Rebate Percent</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="5"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LastYearsIgaPurchaseAmount" type="xs:integer" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Last Years Iga Purchase Amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CoopAllowanceRate" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Co-Op Allowance Rate</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="5"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CoopAllowanceUnitMeasure" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Co-Op Allowance Unit Measure</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ProofOfInsuranceInd" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Proof Of Insurance Indicator</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CommodityGuaranteeInd" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Commodity Guarantee Indicator</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ProofOfInsuranceExpnDt" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Proof Of Insurance Expiration Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorSetupByUserId" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Which Used Id set up the Vendor</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorSiteData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to support information about supplier site level attributes.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="VendorSiteType">
							<xs:sequence>
								<xs:element name="SiteRecordAuditData" type="RecordAuditData" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="VendorEmpMaintInd" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor employee maintenance indicator. Use to hold/control updates from HR application to the Accounting System. Value True/Yes - Ignore updates; False/No - Implement updates.  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorRecordAuditData" type="RecordAuditData" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VendorOutletType">
		<xs:annotation>
			<xs:documentation>Outlet Level Vendor. This is one level below corporate in company's Vendor structure. In SSIMS this type of data is being stored in APVMAS table.</xs:documentation>
