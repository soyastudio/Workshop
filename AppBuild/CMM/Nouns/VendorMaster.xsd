					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorWarehouse" type="VendorWarehouseType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="VendorBackdoorReceiving" type="VendorBackdoorReceivingType" minOccurs="0" maxOccurs="unbounded"/>
