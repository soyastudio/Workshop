	<xs:complexType name="CreditCardType">
		<xs:annotation>
			<xs:documentation>CreditCardType will contain employee credit card information such as merchant, credit card name,  cardnumber, expiration dates,  CVC number, amounts, etc. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CreditCardNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Credit card name</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CreditCardNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>It is the number of the credit card or cardholder?s credit account. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomerNm" type="PersonNameType" minOccurs="0"/>
			<xs:element name="ExpirationDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>MMYY for when the cardholder?s credit card expires.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CreditCardCd" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Thie card code, also called CVC, is usually between 3 and 6 characters long and will vary depending on the type of credit card. For VISA and Mastercard the CVC is often a 3 digit code printed on the reverse side of the card. American express often print a 4 digit code on the front side of the card. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Merchant" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="MerchantId" type="CodeWithDescription" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is your merchant services account identifier.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MerchantNm" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ex: PAYPAL INC MARK</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="200"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="MerchantAddress" type="AddressType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="AmountType" minOccurs="0"/>
			<xs:element name="Amount" type="AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the numerical value of the amount for transaction to or from the cardholder?s credit card account. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillingAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The billing address for purposes of address verification when processing a credit card</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShippingAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The shipping address for payment transactions</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ImageRequiredInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator is say if image is needed for credit card expenses. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BilledDescription" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PinNbr" type="xs:integer" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BankType">
		<xs:annotation>
			<xs:documentation>Banktype will be used to store customer bank attributes such as bank codes, account numbers, bank routing numbers, bank adddress, region, branch information, account balance</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CompanyId" minOccurs="0"/>
			<xs:element name="BankCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation> A user-defined code representing a bank     account at a bank location. Also referred to as Cash Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAccntNbr" type="CodeWithDescription" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The bank-assigned account number identification to be used for the cash code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankId" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the ID number assigned to the bank by the Federal Reserve.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the bank name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankTransactionCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The bank transaction code. Represents a type of cash payment, bill of exchange    payment, or bank service. Also refered to as Bank-Inst code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransactionCdType" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the type of bank transaction Type . C = Cash Payment Transaction.  D = Bill of Exchange Payment Transaction. B = Bank Service Transaction.        </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransactionNbr" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique number used to identify bank transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAccntType" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the type of bank account used to deposit electronic payments for the vendor. Valid values are: C = Current/Checking Account.  S = Savings Account. Blank = Electronic payment type not  selected.          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransactionType" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the type of bank transaction. type Valid values are:  CR = Bank Credit.            DR= Bank Debit.    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankTransitNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A routing transit number is a nine-digit number used to identify a bank or financial institution when clearing funds for electronic transfers or processing checks in the United States.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EncryptedBankTransit" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An encrypted BankTransitNbr. Used to identify a bank or financial institution when clearing fund for electronic transfers or processing checks in the United States.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankEFTNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Branch electronic fund transfer user number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankFormCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code used to drive the invoice number generation for transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAccntBalances" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CheckingAccountBalance" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Customer check account balance</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SavingsAccountBalance" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Customer saving account balance</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CurrentBalance" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The current  balance of the bank account.  </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ReconciledBalance" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>lThe current reconciled balance of the bank account.  </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BankAccntAmounts" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="IssuedBankAmt" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Customer account balance</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BaseCurrencyIssuedAmt" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Customer account balance</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ReconciledAmt" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>BalThe current reconciled balance of the bank account.  </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BaseCurrencyReconciledAmt" type="AmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>BalThe current reconciled balance of the bank account.  </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BankBranch" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank Branch information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankRegion" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank region information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAddress" type="AddressType" minOccurs="0"/>
			<xs:element name="AutomaticClearingHouseInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the value that indicates if the bank account allows ACH payments to   
be made. N = No ACH allowed  Y = ACH allowed</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankCurrencyCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The currency of the bank account.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankCurrencyExchangeRt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The exchange rate used to convert the transaction currency to the bank account currency.                   </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="18"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankEntity" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The bank location where the bank account is located</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendBankEntity" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the ID assigned by the Federal Reserve to the bank that processes the ACH tape.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendBankNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the name of the sending DFI bank.     </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankAccntStatus" type="StatusType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The status of the of the bank account. If the bank account is Inactive, new bank transactions cannot be entered for the bank account, but bank transactions already entered in the system can be processed and reconciled. Valid values
are: A = Active. I = Inactive.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankCodeInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the cash code has been used. Valid values are:  Y = Yes. N = No.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BankClsCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank Class Code - The user class assigned to the cash code. Used to restrict access and use of the cash code to a specific group of
users. If left blank, no access
restrictions apply.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IBANId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>International bank account number.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
