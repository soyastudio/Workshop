			<xs:element name="Division" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ns2:DivisionType">
							<xs:sequence>
								<xs:element name="OfficeCorporationId" minOccurs="0">
