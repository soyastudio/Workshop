					<xs:documentation>Last Interior Decor Update Type</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LastInteriorDecorUpdDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Last Interior Decor Update Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastInteriorDecorUpdCost" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Last Interior Decor Update Cost</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FacilityMgrUserId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Facility Manager User Id</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FacilityMgrEmpId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Facility Manager Employee Id</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MarketingArea" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Marketing Area</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MasterMerchantNbr" minOccurs="0">
