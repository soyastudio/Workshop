	<xs:complexType name="OMDistrictType">
		<xs:annotation>
			<xs:documentation>A district is a group of facilities. Retail facilities will be grouped into "Districts", so that the Distri
Manager is able to personally visit the facilities on a timely basis. Manufacturing plants and non-operating facilites also grouped to a district level.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ns2:CorporationId"/>
			<xs:element ref="ns2:DivisionId"/>
			<xs:element name="OperatingAreaCd" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DistrictId">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DistrictNm">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="250"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DistrictType" type="ns2:CodeWithName" minOccurs="0"/>
			<xs:element name="EffectivePeriod" type="EffectivePeriodType">
				<xs:annotation>
					<xs:documentation>Time period in effective for a version of the record.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActivePeriod" type="ActivePeriodType">
				<xs:annotation>
					<xs:documentation>Time period during which the OM component is active.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="OfficeFacilityId" minOccurs="0"/>
			<xs:element name="OMResponsibleParty" type="ResponsibleParty" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AccountingType" type="AccountingComponentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Accounting structure assigned to the the OM componenet in financial system. ex COA structure</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DistrictStatus" type="ns2:StatusType" minOccurs="0"/>
			<xs:element name="RecordAuditData" type="ns2:RecordAuditData" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OMOeparatingAreaType"/>
	<xs:complexType name="OMDepartmentType">
		<xs:annotation>
			<xs:documentation> Used for identifying departments within facilities. Department will only be valid for particular facility types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DepartmentCd" type="xs:string">
				<xs:annotation>
					<xs:documentation>Standard 3 character code 
that is used with corporation code, division code and facility code to uniquely identify a department. 
A department is a distinct part of a facility's  operation, or an administrative unit of a facility.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartmentNm" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation> standard name for a department.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartmentShortNm" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>standard abbreviated name for a department</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartmentType" type="ns2:CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation> Identifies a type of a department. 
Example department types are: BACKSTAGE, 
PROCESSING, RETAIL, TRANSPORTATION and 
WAREHOUSE</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EffectivePeriodType">
		<xs:annotation>
			<xs:documentation>Time period in effective for a version of the record.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StartDt" type="xs:date" minOccurs="0"/>
			<xs:element name="EndDt" type="xs:date" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ActivePeriodType">
		<xs:annotation>
			<xs:documentation>Time period during which the OM component is active.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FirstActiveDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when a OM component like Corp, Div or facility is first active.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastActiveDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when a OM component like Corp, Div or facility is last active.
generally defaulted to 9999-12-31 for all active OM components.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccountingComponentType">
		<xs:annotation>
			<xs:documentation>Accounting components assigned the the OM componenet in financial system. ex COA structure</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="ComponentType" type="ns2:CodeWithName" minOccurs="0"/>
			<xs:element name="ComponentValueTxt" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AlternateIdType" type="ns2:CodeWithName"/>
	<xs:element name="OfficeFacilityId">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>