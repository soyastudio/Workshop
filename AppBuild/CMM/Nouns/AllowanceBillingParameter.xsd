			</xs:element>
			<xs:element name="ProcessDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Process date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CancelDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Cancel date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SectionCd" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Defines the Cd that uniquely identified the  grouping of products or services within a department.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorDealStatusCd" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor deal status code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
