	<xs:include schemaLocation="../Nouns/FinanceMaster.xsd"/>
	<xs:include schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>
	<xs:complexType name="AllowanceBillingParameterType">
		<xs:annotation>
			<xs:documentation>Standard Albertson companies schema for Billing record offer allowance parameters.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AllowanceBillingHeader" type="AllowanceBillingHeaderType"/>
			<xs:element name="AllowanceBillingItemDetail" type="AllowanceBillingItemDetailType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AllowanceBillingHeaderType">
		<xs:annotation>
			<xs:documentation>It represents billing records header.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BillingRecordId">
				<xs:annotation>
					<xs:documentation>Unique identifier generated by CABS to track individual billing records.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="22"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="DivisionId"/>
			<xs:element name="AllowanceTypeCd">
				<xs:annotation>
					<xs:documentation>Allowance type code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LocationListInd">
				<xs:annotation>
					<xs:documentation>Location list indicator. D - No rog or store table populated, R - only ROG populated, S - only store populated. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Location" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Location list. It can have a ROG list or a Store list associated with the Billing Record but not both.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice minOccurs="0">
						<xs:element name="ROG" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>ROG list.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="0">
									<xs:element ref="ROGCd" minOccurs="0"/>
									<xs:element ref="EffectiveTimePeriod" minOccurs="0"/>
									<xs:element name="CreateTs" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Time stamp of when message was created at source.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CreateUserId" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Id of the client that created the message at source.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="LastUpdateTs" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Date and time stamp of when message was last modified at source.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LastUpdateUserId" minOccurs="0">
										<xs:annotation>
											<xs:documentation>User Id of who last modified the message at source.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Store" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Store list.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="0">
									<xs:element ref="RetailStoreId" minOccurs="0"/>
									<xs:element ref="EffectiveTimePeriod" minOccurs="0"/>
									<xs:element name="CreateTs" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Time stamp of when message was created at source.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CreateUserId" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Id of the client that created the message at source.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="LastUpdateTs" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Date and time stamp of when message was last modified at source.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LastUpdateUserId" minOccurs="0">
										<xs:annotation>
											<xs:documentation>User Id of who last modified the message at source.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="State" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>State list.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="StateCd" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element ref="EffectiveTimePeriod" minOccurs="0"/>
						<xs:element name="CreateTs" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Time stamp of when message was created at source.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CreateUserId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Id of the client that created the message at source.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="LastUpdateTs" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Date and time stamp of when message was last modified at source.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LastUpdateUserId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>User Id of who last modified the message at source.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BillingRecordStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing record status. Expected values: New, Analyze Later, Ready for Income, Completed, Ended, Non-CABS.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="BillingRecordStatusCd" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="BillingRecordStatusDsc" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="VendorOfferNbr" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor original Offer number captured in NOPA/eDeals</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorOfferDsc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Description of the vendor original Offer.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PerformanceCd1">
				<xs:annotation>
					<xs:documentation>Performance code 1.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PerformanceCd2">
				<xs:annotation>
					<xs:documentation>Performance code 2.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FlatPerformanceCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flat performance code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DealLocationId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Location Id of the deal from Vendor view point. This could be a Division Id along with other localized information.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="22"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PayeeTypeCd">
				<xs:annotation>
					<xs:documentation>Payee type code. D - Deduct (AP Number ) or I - Invoice (AR number ).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorPayeeAccountNbr" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Accounts Payable Vendor  Number.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VendorPayeeAccountNm" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Accounts Payable vendor account name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CustomerAccountNbr" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Accounts Receivable customer number.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CustomerAccountNm" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Accounts Receivable customer account name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="VendorNm" minOccurs="0"/>
			<xs:element name="FlatAllowanceAmt" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flat allowance amount.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="15"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BillingStartDt" type="DateBaseType" nillable="true">
				<xs:annotation>
					<xs:documentation>Billing start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillingEndDt" type="DateBaseType" nillable="true">
				<xs:annotation>
					<xs:documentation>Billing end date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderStartDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Order start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderEndDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Order end date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShippingStartDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipping start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShippingEndDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipping end date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalStartDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Arrival start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalEndDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Arrival end date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AllowanceStartDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Allowance start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AllowanceEndDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Allowance end date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PerformanceStartDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Performance start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PerformanceEndDt" type="DateBaseType" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Performance end date.</xs:documentation>
