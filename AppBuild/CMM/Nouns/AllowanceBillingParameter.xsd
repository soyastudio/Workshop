						</xs:element>
						<xs:element name="StateListChangeInd" minOccurs="1">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ReviewStatusChangeInd" minOccurs="1">
