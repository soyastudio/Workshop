<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSpy v2021 rel. 3 (x64) (http://www.altova.com) by mgadd09 (Albertsons) --><!-- edited with XMLSpy v2021 rel. 2 sp1 (x64) (http://www.altova.com) by Alex Nunes (Albertsons) --><!-- edited with XMLSpy v2021 rel. 3 (x64) (http://www.altova.com) by Sridhar Katkuri (Safeway) --><xs:schema xmlns="http://collab.safeway.com/it/architecture/info/default.aspx" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:Abs="http://collab.safeway.com/it/architecture/info/default.aspx" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" targetNamespace="http://collab.safeway.com/it/architecture/info/default.aspx" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">	<!--v1.0 - Monica Gaddam - Initial Version-->	<xs:include schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>	<xs:element name="MerchandiseResponsibleOrganization" type="MerchandiseResponsibleOrganizationType"/>	<xs:complexType name="MerchandiseResponsibleOrganizationType">		<xs:annotation>			<xs:documentation>Identifies the VP, Director, category manager, pricer and other codes related to each combination of group_cd and catgry_cd, within the corporation and division at Product, Divisional and National Level.</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element ref="CorporationId"/>			<xs:element ref="DivisionId"/>			<xs:element name="SMICGroup" type="IdWithNameType">				<xs:annotation>					<xs:documentation>SMIC group code for tax reporting, Also known as lifo pool cd.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="SMICCategory" type="IdWithNameType">				<xs:annotation>					<xs:documentation>Unique Identifier of a SMIC Category.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="SMIC">				<xs:annotation>					<xs:documentation>Contains Safeway Merchandising Item Classification(SMIC) data.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="SMICGroupCd" type="xs:short" nillable="false" minOccurs="0">							<xs:annotation>								<xs:documentation>The unique Id of a SMIC category, derived from Group Cd + Category Cd + Class Cd + Sub Class Cd + Sub Sub Class Cd. Group Cd is the first level in the SMIC classification for the Smatch system. It must exist on MEPRDCLF where the Category, Class, Sub Class, and Sub-Sub Class are zero. SMIC = Safeway Merchandising Item Cd.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="SMICCategoryCd" type="xs:short" nillable="false" minOccurs="0">							<xs:annotation>								<xs:documentation>The unique Id of a SMIC category derived from Group Cd + Category Cd + Class Cd + Sub Class Cd + Sub Sub Class Cd. Category Cd is the second level in the SMIC classification for the Smatch system. SMIC = Safeway Merchandising Item Cd.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="SMICClassCd" type="xs:short" nillable="false" minOccurs="0">							<xs:annotation>								<xs:documentation>The unique Id of a SMIC category derived from Group Cd + Category Cd + Class Cd + Sub Class Cd + Sub Sub Class Cd. Class Cd is the third level in the SMIC classification for the Smatch system. SMIC = Safeway Merchandising Item Cd.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="SMICSubClassCd" type="xs:short" nillable="false" minOccurs="0">							<xs:annotation>								<xs:documentation>The unique Id of a SMIC category, derived from Group Cd + Category Cd + Class Cd + Sub Class Cd + Sub Sub Class Cd. Sub Class Cd is the fourth level in the SMIC classification for the Smatch system. SMIC = Safeway Merchandising Item Cd.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="SMICSub-SubClassCd" type="xs:short" nillable="false" minOccurs="0">							<xs:annotation>								<xs:documentation>The unique Id of a SMIC category, derived from Group Cd + Category Cd + Class Cd + Sub Class Cd + Sub Sub Class Cd. Sub Sub Class Cd is the fifth level in the SMIC classification for the Smatch system. SMIC = Safeway Merchandising Item Cd.</xs:documentation>							</xs:annotation>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="ResponsibleGroup" type="ResponsibleGroupType" minOccurs="0"/>			<xs:element name="SmicHighBillingGrossLimitPct" type="xs:decimal" minOccurs="0">				<xs:annotation>					<xs:documentation>This is the high end of the range for Billing Gross Percentage.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="SmicLowBillingGrossLimitPct" type="xs:decimal" minOccurs="0">				<xs:annotation>					<xs:documentation>This is the low end range for billing Gross Percentage</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="DivisionalCategoryAnalystNbr" type="xs:integer" minOccurs="0"/>			<xs:element name="BusinessUnitType" minOccurs="0">				<xs:complexType>					<xs:sequence>						<xs:element name="BusinessUnitId" type="xs:integer" minOccurs="0"/>						<xs:element name="BusinessUnitNm" minOccurs="0">							<xs:annotation>								<xs:documentation>Business unit: a group of retail sections make up a business unit. E.G. the Cheese, Refrigerated, Milk, and Eggs retail sections make up the Dairy/Refrigerated business unit.</xs:documentation>							</xs:annotation>							<xs:simpleType>								<xs:restriction base="xs:string">									<xs:maxLength value="250"/>								</xs:restriction>							</xs:simpleType>						</xs:element>						<xs:element name="BusinessGroupId" type="xs:integer" minOccurs="0"/>						<xs:element name="BusinessGroupNm" minOccurs="0">							<xs:annotation>								<xs:documentation>Business group: a group of business units make up a business group. E.G. the Dairy/Refrigerated and Frozen business units make up the Dairy/Refrigerated/Frozen business group.</xs:documentation>							</xs:annotation>							<xs:simpleType>								<xs:restriction base="xs:string">									<xs:maxLength value="250"/>								</xs:restriction>							</xs:simpleType>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="DefaultPageModeCd" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation>The mode to be used in the pricing screeens ADC876 and ADC8700 to move from item to item. There are 3 values : 'D'  Next Date 'R'  Next ROG'V'  Next Vendor</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="PricerNbr" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation>Number assigned to the Pricer</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="MinimumDayCostChargeCnt" type="xs:integer" minOccurs="0">				<xs:annotation>					<xs:documentation>A guideline for the minimum number of days in the future for an effective date.  Default is 14 days.  It is assigned by SMIC. Must be numeric and a positive value.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="CostChangeVariancePct" type="xs:decimal" minOccurs="0">				<xs:annotation>					<xs:documentation>A guideline for the maximum percentage increase or decrease in cost. Default is 10%. It is assigned by SMIC and must be a positive number.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="CostChangeLimitPct" type="xs:decimal" minOccurs="0">				<xs:annotation>					<xs:documentation>Maximum variance or limit allowed for a cost change. Usage: When a cost change variance exceeds the guideline (the value in CST_CHG_VAR_PCT), it flags a warning and when it exceeds the LIMIT (the value in this column) it flags an error.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="PromotionPricerNbr" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation>This column allows two different Pricers to be associated with SMICs by division. The column would relate to a Promotion Pricing Specialist. </xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="AllowanceOffInvoiceInCostDefaultSw" type="xs:boolean" minOccurs="0">				<xs:annotation>					<xs:documentation>Y - this SMIC group must have off invoice allowances calculated as part of the cost that is sent to DemandTec by default :  N or blank indicates that this item does not need allowances included in the cost by default. Items can be modified on an individual basis to say otherwise. </xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ResponsibleGroupType">		<xs:sequence>			<xs:element name="SeniorVicePresident" minOccurs="0">				<xs:complexType>					<xs:sequence>						<xs:element name="SeniorVicePresidentCd" type="xs:string" minOccurs="0">							<xs:annotation>								<xs:documentation>Senior Vice President Code</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="SeniorVicePresidentNm" type="xs:string" minOccurs="0">							<xs:annotation>								<xs:documentation>Senior Vice President Name</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="VicePresident" minOccurs="0" maxOccurs="unbounded">							<xs:complexType>								<xs:sequence>									<xs:element name="VicePresidentCd" type="xs:string" minOccurs="0">										<xs:annotation>											<xs:documentation>Vice President Code</xs:documentation>										</xs:annotation>									</xs:element>									<xs:element name="VicePresidentNm" type="xs:string" minOccurs="0">										<xs:annotation>											<xs:documentation>Vice President Name</xs:documentation>										</xs:annotation>									</xs:element>									<xs:element name="Director" minOccurs="0" maxOccurs="unbounded">										<xs:complexType>											<xs:sequence minOccurs="0">												<xs:element name="DirectorCd" minOccurs="0">													<xs:annotation>														<xs:documentation>Director Code</xs:documentation>													</xs:annotation>													<xs:simpleType>														<xs:restriction base="xs:string">															<xs:maxLength value="2"/>														</xs:restriction>													</xs:simpleType>												</xs:element>												<xs:element name="DirectorNm" minOccurs="0">													<xs:annotation>														<xs:documentation>Director Name</xs:documentation>													</xs:annotation>													<xs:simpleType>														<xs:restriction base="xs:string">															<xs:maxLength value="23"/>														</xs:restriction>													</xs:simpleType>												</xs:element>												<xs:element name="CategoryManager" minOccurs="0" maxOccurs="unbounded">													<xs:complexType>														<xs:sequence>															<xs:element name="ManagerCd" type="xs:string" minOccurs="0">																<xs:annotation>																	<xs:documentation>The unique identifying number assigned to the Category Manager.</xs:documentation>																</xs:annotation>															</xs:element>															<xs:element name="ManagerNm" type="xs:string" minOccurs="0"/>															<xs:element name="ManagerUserId" minOccurs="0"/>														</xs:sequence>													</xs:complexType>												</xs:element>											</xs:sequence>										</xs:complexType>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="ResponsibleGroupType" type="xs:string"/>		</xs:sequence>	</xs:complexType></xs:schema>